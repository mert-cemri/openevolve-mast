    def get_memory_info(self):
        '''Fetches overall system memory usage.'''
        with open('/proc/meminfo', 'r') as file:
            meminfo = file.readlines()
        memory_data = {}
        for line in meminfo:
            parts = line.split(':')
            memory_data[parts[0]] = parts[1].strip()
        return memory_data
