                messagebox.showinfo("Success", f"File saved to {output_path}")
            else:
                messagebox.showinfo("Output", numbered_content)
        
        except Exception as e:
            messagebox.showerror("Error", str(e))

if __name__ == "__main__":
    MainApp()
