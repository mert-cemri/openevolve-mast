'''

Main application file for the CLI flashcard study tool using tkinter for GUI.

'''

import tkinter as tk
from tkinter import simpledialog, messagebox
from deck import Deck
from flashcard import Flashcard

class FlashcardApp:
    def __init__(self, root):
        self.root = root
        self.root.title("Flashcard Study Tool")
        self.decks = {}
        
        self.current_deck = None
        self.current_flashcard_index = 0
        self.is_front = True

        self.setup_gui()

    def setup_gui(self):
        self.deck_label = tk.Label(self.root, text="No deck selected")
        self.deck_label.pack()

        self.flashcard_text = tk.Label(self.root, text="", font=("Arial", 24))
        self.flashcard_text.pack()

        self.flip_button = tk.Button(self.root, text="Flip", command=self.flip_flashcard)
        self.flip_button.pack()

        self.next_button = tk.Button(self.root, text="Next", command=self.show_next_flashcard)
        self.next_button.pack()

        self.create_deck_button = tk.Button(self.root, text="Create Deck", command=self.create_deck)
        self.create_deck_button.pack()

        self.add_flashcard_button = tk.Button(self.root, text="Add Flashcard", command=self.add_flashcard_to_deck)
        self.add_flashcard_button.pack()

        self.review_deck_button = tk.Button(self.root, text="Review Deck", command=self.review_deck)
        self.review_deck_button.pack()

    def create_deck(self):
        deck_name = simpledialog.askstring("Input", "Enter deck name:")
        if deck_name:
            self.decks[deck_name] = Deck(deck_name)
            messagebox.showinfo("Info", f"Deck '{deck_name}' created.")

    def add_flashcard_to_deck(self):
        if not self.decks:
            messagebox.showwarning("Warning", "No decks available. Create a deck first.")
            return

        deck_name = simpledialog.askstring("Input", "Enter deck name:")
        if deck_name in self.decks:
            front = simpledialog.askstring("Input", "Enter flashcard front:")
            back = simpledialog.askstring("Input", "Enter flashcard back:")
            flashcard = Flashcard(front, back)
            self.decks[deck_name].add_flashcard(flashcard)
            messagebox.showinfo("Info", "Flashcard added.")
        else:
            messagebox.showwarning("Warning", "Deck not found.")

    def review_deck(self):
        if not self.decks:
            messagebox.showwarning("Warning", "No decks available. Create a deck first.")
            return

        deck_name = simpledialog.askstring("Input", "Enter deck name:")
        if deck_name in self.decks:
            self.current_deck = self.decks[deck_name]
            self.current_flashcard_index = 0
            self.is_front = True
            self.show_flashcard()
        else:
            messagebox.showwarning("Warning", "Deck not found.")

    def show_flashcard(self):
        if self.current_deck and self.current_deck.get_flashcards():
            flashcard = self.current_deck.get_flashcards()[self.current_flashcard_index]
            text = flashcard.get_front() if self.is_front else flashcard.get_back()
            self.flashcard_text.config(text=text)
            self.deck_label.config(text=f"Deck: {self.current_deck.name}")
        else:
            self.flashcard_text.config(text="No flashcards available.")

    def flip_flashcard(self):
        self.is_front = not self.is_front
        self.show_flashcard()

    def show_next_flashcard(self):
        if self.current_deck and self.current_deck.get_flashcards():
            self.current_flashcard_index = (self.current_flashcard_index + 1) % len(self.current_deck.get_flashcards())
            self.is_front = True
            self.show_flashcard()

if __name__ == "__main__":
    root = tk.Tk()
    app = FlashcardApp(root)
    root.mainloop()
```

