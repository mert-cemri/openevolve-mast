        self.output_text = tk.Text(root, wrap='word', height=20, width=60)
        self.output_text.pack(pady=10)
        
        self.save_button = tk.Button(root, text="Save to File", command=self.save_to_file)
        self.save_button.pack(pady=5)
        
        self.pdf_path = None

    def select_pdf(self):
        '''
        Open a file dialog to select a PDF file and extract its text.
        '''
        self.pdf_path = filedialog.askopenfilename(filetypes=[("PDF Files", "*.pdf")])
        if self.pdf_path:
            text = extract_text_from_pdf(self.pdf_path)
            self.output_text.delete(1.0, tk.END)
            self.output_text.insert(tk.END, text)

