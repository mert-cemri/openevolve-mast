import tkinter as tk
from tkinter import filedialog, messagebox
from text_converter import to_uppercase, to_lowercase, to_titlecase, to_sentencecase

class TextCaseConverterApp:
    def __init__(self, root):
        self.root = root
        self.root.title("Text Case Converter")

        self.input_text = tk.StringVar()
        self.output_text = tk.StringVar()

        self.create_widgets()

    def create_widgets(self):
        tk.Label(self.root, text="Input Text:").pack()
        tk.Entry(self.root, textvariable=self.input_text, width=50).pack()

        tk.Button(self.root, text="Open File", command=self.open_file).pack()

        self.case_option = tk.StringVar(value="uppercase")
        tk.Radiobutton(self.root, text="Uppercase", variable=self.case_option, value="uppercase").pack()
        tk.Radiobutton(self.root, text="Lowercase", variable=self.case_option, value="lowercase").pack()
        tk.Radiobutton(self.root, text="Title Case", variable=self.case_option, value="titlecase").pack()
        tk.Radiobutton(self.root, text="Sentence Case", variable=self.case_option, value="sentencecase").pack()

        tk.Button(self.root, text="Convert", command=self.convert_text).pack()

        tk.Label(self.root, text="Output Text:").pack()
        tk.Entry(self.root, textvariable=self.output_text, width=50).pack()

        tk.Button(self.root, text="Save to File", command=lambda: self.save_file(self.output_text.get())).pack()

    def convert_text(self):
        text = self.input_text.get()
        case = self.case_option.get()

        if case == "uppercase":
            converted_text = to_uppercase(text)
        elif case == "lowercase":
            converted_text = to_lowercase(text)
        elif case == "titlecase":
            converted_text = to_titlecase(text)
        elif case == "sentencecase":
            converted_text = to_sentencecase(text)
        else:
            converted_text = text

        self.output_text.set(converted_text)

    def open_file(self):
        file_path = filedialog.askopenfilename()
        if file_path:
            with open(file_path, 'r') as file:
                content = file.read()
                self.input_text.set(content)

    def save_file(self, text):
        file_path = filedialog.asksaveasfilename(defaultextension=".txt")
        if file_path:
            with open(file_path, 'w') as file:
                file.write(text)
            messagebox.showinfo("Success", "File saved successfully!")

if __name__ == "__main__":
    root = tk.Tk()
    app = TextCaseConverterApp(root)
    root.mainloop()
