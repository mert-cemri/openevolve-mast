'''

FILENAME: main.py

'''

import tkinter as tk
import re
from tkinter import messagebox

class RegexTesterApp:
    '''
    DOCSTRING: This class creates a GUI application for testing regular expressions against a test string.
    '''

    def __init__(self, root):
        '''
        DOCSTRING: Initializes the GUI components.
        '''
        self.root = root
        self.root.title("Regex Tester")
        self.create_widgets()

    def create_widgets(self):
        '''
        DOCSTRING: Creates and places the widgets in the GUI.
        '''
        # Labels
        self.regex_label = tk.Label(self.root, text="Regular Expression:")
        self.regex_label.grid(row=0, column=0, padx=10, pady=5)

        self.string_label = tk.Label(self.root, text="Test String:")
        self.string_label.grid(row=1, column=0, padx=10, pady=5)

        # Entry fields
        self.regex_entry = tk.Entry(self.root, width=50)
        self.regex_entry.grid(row=0, column=1, padx=10, pady=5)

        self.string_entry = tk.Entry(self.root, width=50)
        self.string_entry.grid(row=1, column=1, padx=10, pady=5)

        # Check button
        self.check_button = tk.Button(self.root, text="Check", command=self.check_regex)
        self.check_button.grid(row=2, column=0, columnspan=2, pady=10)

        # Result display
        self.result_label = tk.Label(self.root, text="", fg="blue")
        self.result_label.grid(row=3, column=0, columnspan=2, padx=10, pady=5)

    def check_regex(self):
        '''
        DOCSTRING: Checks if the test string matches the regular expression and displays the result.
        '''
        regex = self.regex_entry.get()
        test_string = self.string_entry.get()

        try:
            pattern = re.compile(regex)
            match = pattern.match(test_string)

            if match:
                result_text = "Match found!"
                groups = match.groups()
                if groups:
                    result_text += f" Matched groups: {groups}"
            else:
                result_text = "No match found."

            self.display_results(result_text)

        except re.error as e:
            messagebox.showerror("Invalid Regex", f"Error in regular expression: {e}")

    def display_results(self, result_text):
        '''
        DOCSTRING: Displays the result of the regex test in the GUI.
        '''
        self.result_label.config(text=result_text)

    def run(self):
        '''
        DOCSTRING: Starts the main application loop.
        '''
        self.root.mainloop()

if __name__ == "__main__":
    root = tk.Tk()
    app = RegexTesterApp(root)
    app.run()
```

