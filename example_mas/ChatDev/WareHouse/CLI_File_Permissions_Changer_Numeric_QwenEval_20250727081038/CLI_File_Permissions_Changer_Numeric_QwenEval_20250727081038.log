import tkinter as tk
from tkinter import messagebox, filedialog
from permission_changer import change_permissions

class PermissionGUI(tk.Tk):
    def __init__(self):
        super().__init__()
        self.title("Change File Permissions")
        self.geometry("400x200")

        self.file_path_var = tk.StringVar()
        self.permission_mode_var = tk.StringVar()

        self.create_widgets()

    def create_widgets(self):
        tk.Label(self, text="File Path:").grid(row=0, column=0, padx=10, pady=10)
        tk.Entry(self, textvariable=self.file_path_var, width=40).grid(row=0, column=1, padx=10, pady=10)
        tk.Button(self, text="Browse", command=self.browse_file).grid(row=0, column=2, padx=10, pady=10)

        tk.Label(self, text="Permission Mode (e.g., 755):").grid(row=1, column=0, padx=10, pady=10)
        tk.Entry(self, textvariable=self.permission_mode_var, width=40).grid(row=1, column=1, padx=10, pady=10)

        tk.Button(self, text="Change Permissions", command=self.on_change_permissions).grid(row=2, column=1, pady=20)

    def browse_file(self):
        file_path = filedialog.askopenfilename()
        self.file_path_var.set(file_path)

    def on_change_permissions(self):
        file_path = self.file_path_var.get()
        permission_mode = self.permission_mode_var.get()

        if not file_path or not permission_mode:
            messagebox.showerror("Error", "Please provide both file path and permission mode.")
            return

        try:
            change_permissions(file_path, permission_mode)
            messagebox.showinfo("Success", f"Permissions for {file_path} changed to {permission_mode}.")
        except Exception as e:
            messagebox.showerror("Error", str(e))

