'''

This module provides a GUI tool for encoding and decoding strings using Base64.
The user can input a string and choose whether to encode or decode it.

'''

import tkinter as tk
from tkinter import ttk
import base64

class Base64Tool:
    def __init__(self):
        self.root = tk.Tk()
        self.root.title("Base64 Encoder/Decoder")

        # Input field
        self.input_label = ttk.Label(self.root, text="Input String:")
        self.input_label.grid(column=0, row=0, padx=10, pady=10)
        self.input_entry = ttk.Entry(self.root, width=50)
        self.input_entry.grid(column=1, row=0, padx=10, pady=10)

        # Output field
        self.output_label = ttk.Label(self.root, text="Output String:")
        self.output_label.grid(column=0, row=1, padx=10, pady=10)
        self.output_entry = ttk.Entry(self.root, width=50)
        self.output_entry.grid(column=1, row=1, padx=10, pady=10)

        # Radio buttons for encode/decode
        self.operation = tk.StringVar(value="encode")
        self.encode_radio = ttk.Radiobutton(self.root, text="Encode", variable=self.operation, value="encode")
        self.encode_radio.grid(column=0, row=2, padx=10, pady=10)
        self.decode_radio = ttk.Radiobutton(self.root, text="Decode", variable=self.operation, value="decode")
        self.decode_radio.grid(column=1, row=2, padx=10, pady=10)

        # Process button
        self.process_button = ttk.Button(self.root, text="Process", command=self.process_input)
        self.process_button.grid(column=0, row=3, columnspan=2, pady=10)

    def encode_string(self, input_string):
        '''Encodes the input string using Base64.'''
        encoded_bytes = base64.b64encode(input_string.encode('utf-8'))
        return encoded_bytes.decode('utf-8')

    def decode_string(self, input_string):
        '''Decodes the input string using Base64.'''
        try:
            decoded_bytes = base64.b64decode(input_string.encode('utf-8'))
            return decoded_bytes.decode('utf-8')
        except Exception as e:
            return f"Error: {str(e)}"

    def process_input(self):
        '''Processes the input based on the selected operation (encode/decode).'''
        input_string = self.input_entry.get()
        if self.operation.get() == "encode":
            output_string = self.encode_string(input_string)
        else:
            output_string = self.decode_string(input_string)
        self.output_entry.delete(0, tk.END)
        self.output_entry.insert(0, output_string)

def main():
    '''Entry point of the application.'''
    app = Base64Tool()
    app.root.mainloop()

if __name__ == "__main__":
    main()
```

