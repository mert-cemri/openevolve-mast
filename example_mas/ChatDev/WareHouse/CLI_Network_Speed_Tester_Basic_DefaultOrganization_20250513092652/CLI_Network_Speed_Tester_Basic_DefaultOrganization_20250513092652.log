'''

Main application file for the CLI network speed tester with GUI.

'''

import tkinter as tk
from tkinter import messagebox
from speed_test import perform_speed_test

class SpeedTesterApp:
    def __init__(self, root):
        '''
        Initialize the SpeedTesterApp with GUI components.
        '''
        self.root = root
        self.root.title("Network Speed Tester")

        self.label = tk.Label(root, text="Press 'Start Test' to measure your network speed")
        self.label.pack(pady=20)

        self.start_button = tk.Button(root, text="Start Test", command=self.run_speed_test)
        self.start_button.pack(pady=10)

        self.result_label = tk.Label(root, text="", font=("Helvetica", 12))
        self.result_label.pack(pady=20)

    def run_speed_test(self):
        '''
        Run the network speed test and update the GUI with results.
        '''
        self.result_label.config(text="Testing...")
        self.root.update_idletasks()
        try:
            download_speed, upload_speed = perform_speed_test()
            result_text = f"Download Speed: {download_speed:.2f} Mbps\nUpload Speed: {upload_speed:.2f} Mbps"
        except Exception as e:
            result_text = f"Error: {str(e)}"
        self.result_label.config(text=result_text)

if __name__ == "__main__":
    root = tk.Tk()
    app = SpeedTesterApp(root)
    root.mainloop()
```

#### speed_test.py

