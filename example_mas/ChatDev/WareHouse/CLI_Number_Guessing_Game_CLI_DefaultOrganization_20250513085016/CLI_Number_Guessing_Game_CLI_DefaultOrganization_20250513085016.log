            if guess < random_number:
                print("Higher!")
            elif guess > random_number:
                print("Lower!")
            else:
                print("Congratulations! You've guessed it!")
                break
        except ValueError:
            print("Please enter a valid number.")
if __name__ == "__main__":
    start_game()
