'''
Regex tester module for the CLI regex tester application.
'''

import re

class RegexTester:
    def __init__(self, regex, test_string):
        self.regex = regex
        self.test_string = test_string

    def test_regex(self):
        try:
            match = re.match(self.regex, self.test_string)
            if match:
                matched_groups = match.groups()
                return True, matched_groups
            else:
                return False, None
        except re.error:
            return False, None

    def get_matched_groups(self):
        match = re.match(self.regex, self.test_string)
        if match:
            return match.groups()
        return None
```

