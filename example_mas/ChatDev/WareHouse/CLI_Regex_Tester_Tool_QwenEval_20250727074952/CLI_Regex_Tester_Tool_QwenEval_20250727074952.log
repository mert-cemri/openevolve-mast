import tkinter as tk
from tkinter import messagebox
from regex_tester import RegexTester

class RegexTesterGUI:
    def __init__(self):
        self.root = tk.Tk()
        self.root.title("Regex Tester")
        self.root.geometry("400x300")

        self.create_widgets()

    def create_widgets(self):
        tk.Label(self.root, text="Regex:").grid(row=0, column=0, padx=10, pady=10)
        self.regex_entry = tk.Entry(self.root, width=50)
        self.regex_entry.grid(row=0, column=1, padx=10, pady=10)

        tk.Label(self.root, text="Test String:").grid(row=1, column=0, padx=10, pady=10)
        self.test_string_entry = tk.Entry(self.root, width=50)
        self.test_string_entry.grid(row=1, column=1, padx=10, pady=10)

        self.test_button = tk.Button(self.root, text="Test", command=self.on_test_click)
        self.test_button.grid(row=2, column=0, columnspan=2, pady=10)

        self.result_label = tk.Label(self.root, text="")
        self.result_label.grid(row=3, column=0, columnspan=2, pady=10)

    def on_test_click(self):
        regex = self.regex_entry.get()
        test_string = self.test_string_entry.get()

        if not regex or not test_string:
            messagebox.showerror("Input Error", "Both regex and test string must be provided.")
            return

        regex_tester = RegexTester(regex, test_string)
        match_result, matched_groups = regex_tester.test_regex()

        if match_result:
            result_text = "Match found!"
            if matched_groups:
                result_text += f"\nMatched groups: {matched_groups}"
        else:
            result_text = "No match found."

        self.result_label.config(text=result_text)

    def run(self):
        self.root.mainloop()
```

