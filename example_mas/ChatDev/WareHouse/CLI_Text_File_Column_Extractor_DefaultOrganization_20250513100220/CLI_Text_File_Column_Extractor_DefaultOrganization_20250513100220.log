'''

Main application file to initialize the GUI and handle user interactions.

'''

import tkinter as tk
from tkinter import filedialog, messagebox
from file_handler import FileHandler
from column_extractor import ColumnExtractor

class MainApp:
    def __init__(self, root):
        self.root = root
        self.root.title("Column Extractor")
        
        self.file_path = tk.StringVar()
        self.columns_input = tk.StringVar()
        
        self.create_widgets()
    
    def create_widgets(self):
        tk.Label(self.root, text="File:").grid(row=0, column=0, padx=10, pady=10)
        tk.Entry(self.root, textvariable=self.file_path, width=50).grid(row=0, column=1, padx=10, pady=10)
        tk.Button(self.root, text="Browse", command=self.open_file).grid(row=0, column=2, padx=10, pady=10)
        
        tk.Label(self.root, text="Columns (numbers or headers):").grid(row=1, column=0, padx=10, pady=10)
        tk.Entry(self.root, textvariable=self.columns_input, width=50).grid(row=1, column=1, padx=10, pady=10)
        
        tk.Button(self.root, text="Extract", command=self.extract_columns).grid(row=2, column=1, pady=20)
        
        self.result_text = tk.Text(self.root, height=15, width=80)
        self.result_text.grid(row=3, column=0, columnspan=3, padx=10, pady=10)
    
    def open_file(self):
        file_path = filedialog.askopenfilename(filetypes=[("CSV files", "*.csv"), ("Text files", "*.txt")])
        if file_path:
            self.file_path.set(file_path)
    
    def extract_columns(self):
        file_path = self.file_path.get()
        columns = self.columns_input.get()
        
        if not file_path or not columns:
            messagebox.showerror("Error", "Please provide both file path and columns.")
            return
        
        try:
            data = FileHandler.read_file(file_path)
            delimiter = ',' if file_path.endswith('.csv') else ' '
            parsed_data = FileHandler.parse_file(data, delimiter)
            
            if columns.isdigit():
                column_numbers = list(map(int, columns.split(',')))
                extracted_data = ColumnExtractor.extract_by_number(parsed_data, column_numbers)
            else:
                column_headers = columns.split(',')
                extracted_data = ColumnExtractor.extract_by_header(parsed_data, column_headers)
            
            self.display_result(extracted_data)
        except Exception as e:
            messagebox.showerror("Error", str(e))
    
    def display_result(self, data):
        self.result_text.delete(1.0, tk.END)
        for row in data:
            self.result_text.insert(tk.END, '\t'.join(row) + '\n')

if __name__ == "__main__":
    root = tk.Tk()
    app = MainApp(root)
    root.mainloop()
```

