import tkinter as tk
from timer import Timer
from countdown import Countdown

class MainApp:
    def __init__(self):
        self.root = tk.Tk()
        self.root.title("Timer and Countdown Tool")
        
        self.timer = Timer()
        self.countdown = Countdown()
        
        self.timer_label = tk.Label(self.root, text="00:00:00", font=("Helvetica", 48))
        self.timer_label.pack()
        
        self.start_timer_button = tk.Button(self.root, text="Start Timer", command=self.start_timer)
        self.start_timer_button.pack()
        
        self.countdown_entry = tk.Entry(self.root)
        self.countdown_entry.pack()
        
        self.start_countdown_button = tk.Button(self.root, text="Start Countdown", command=self.start_countdown)
        self.start_countdown_button.pack()
        
        self.update_timer()
        
        self.root.mainloop()
    
    def start_timer(self):
        self.timer.start()
    
    def start_countdown(self):
        duration_str = self.countdown_entry.get()
        duration = self.parse_duration(duration_str)
        if duration:
            self.countdown.start(duration)
    
    def update_timer(self):
        if self.timer.running:
            self.timer_label.config(text=self.timer.get_time())
        elif self.countdown.running:
            self.timer_label.config(text=self.countdown.get_time())
        self.root.after(1000, self.update_timer)
    
    def parse_duration(self, duration_str):
        # Parses a duration string like "5m", "1h30s" into seconds
        total_seconds = 0
        try:
            if 'h' in duration_str:
                hours, duration_str = duration_str.split('h')
                total_seconds += int(hours) * 3600
            if 'm' in duration_str:
                minutes, duration_str = duration_str.split('m')
                total_seconds += int(minutes) * 60
            if 's' in duration_str:
                seconds = duration_str.replace('s', '')
                total_seconds += int(seconds)
            return total_seconds
        except ValueError:
            return None

if __name__ == "__main__":
    app = MainApp()
```

