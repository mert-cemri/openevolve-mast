import tkinter as tk
from tkinter import filedialog, messagebox
from PIL import Image

class ImageResizerApp:
    def __init__(self, master):
        self.master = master
        master.title("Image Resizer")

        self.label = tk.Label(master, text="Select an image and enter target dimensions:")
        self.label.pack()

        self.open_button = tk.Button(master, text="Open Image", command=self.open_file)
        self.open_button.pack()

        self.width_label = tk.Label(master, text="Target Width:")
        self.width_label.pack()

        self.width_entry = tk.Entry(master)
        self.width_entry.pack()

        self.height_label = tk.Label(master, text="Target Height:")
        self.height_label.pack()

        self.height_entry = tk.Entry(master)
        self.height_entry.pack()

        self.resize_button = tk.Button(master, text="Resize Image", command=self.resize_image)
        self.resize_button.pack()

        self.image_path = None

    def open_file(self):
        self.image_path = filedialog.askopenfilename(filetypes=[("Image files", "*.jpg *.jpeg *.png *.bmp *.gif")])
        if self.image_path:
            messagebox.showinfo("Selected Image", f"Selected: {self.image_path}")

    def resize_image(self):
        if not self.image_path:
            messagebox.showwarning("No Image", "Please select an image first.")
            return

        target_width = self.width_entry.get()
        target_height = self.height_entry.get()

        if not target_width and not target_height:
            messagebox.showwarning("No Dimensions", "Please enter a target width or height.")
            return

        try:
            target_width = int(target_width) if target_width else None
            target_height = int(target_height) if target_height else None

            output_path = filedialog.asksaveasfilename(defaultextension=".jpg", filetypes=[("JPEG", "*.jpg"), ("PNG", "*.png"), ("BMP", "*.bmp"), ("GIF", "*.gif")])
            if output_path:
                resize_image(self.image_path, output_path, target_width, target_height)
                messagebox.showinfo("Success", f"Image saved to {output_path}")
        except ValueError:
            messagebox.showerror("Invalid Input", "Please enter valid integer values for dimensions.")

def resize_image(input_path, output_path, target_width=None, target_height=None):
    with Image.open(input_path) as img:
        original_width, original_height = img.size

        if target_width and not target_height:
            target_height = int((target_width / original_width) * original_height)
        elif target_height and not target_width:
            target_width = int((target_height / original_height) * original_width)

        img = img.resize((target_width, target_height), Image.ANTIALIAS)
        img.save(output_path)

if __name__ == "__main__":
    root = tk.Tk()
    app = ImageResizerApp(root)
    root.mainloop()
```

