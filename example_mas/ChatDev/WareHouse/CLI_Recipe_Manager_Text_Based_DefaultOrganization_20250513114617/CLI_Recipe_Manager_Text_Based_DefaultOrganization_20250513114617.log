                self.recipes = [Recipe(**data) for data in recipes_data]
        except FileNotFoundError:
            self.recipes = []
    def save_recipes(self):
        with open('recipes.json', 'w') as file:
            json.dump([recipe.to_dict() for recipe in self.recipes], file, indent=4)
