import os

class FileSystemUtils:
    @staticmethod
    def get_directory_size(directory):
        if not os.path.exists(directory):
            raise ValueError(f"The directory {directory} does not exist.")
        
        usage_data = {}
        error_count = 0
        for dirpath, dirnames, filenames in os.walk(directory):
