    def get_directory_size(directory):
        usage_data = {}
        for dirpath, dirnames, filenames in os.walk(directory):
            for f in filenames:
                fp = os.path.join(dirpath, f)
                try:
                    size = os.path.getsize(fp)
                    usage_data[fp] = size
                except OSError as e:
                    print(f"Error accessing {fp}: {e}")
        return usage_data
