import tkinter as tk
from tkinter import ttk
import psutil
import threading
import time

class CPUMonitorApp:
    def __init__(self, root, pid=None):
        self.root = root
        self.root.title("CPU Usage Monitor")
        self.pid = pid
        self.tree = ttk.Treeview(root, columns=('PID', 'Name', 'CPU Usage'), show='headings')
        self.tree.heading('PID', text='PID')
        self.tree.heading('Name', text='Name')
        self.tree.heading('CPU Usage', text='CPU Usage (%)')
        self.tree.pack(fill=tk.BOTH, expand=True)
        self.update_interval = 2  # Update every 2 seconds
        self.update_display()

    def get_cpu_usage(self):
        '''Fetches CPU usage for all processes or a specific process if PID is provided.'''
        cpu_usage_data = []
        if self.pid:
            try:
                proc = psutil.Process(self.pid)
                cpu_usage_data.append((proc.pid, proc.name(), proc.cpu_percent(interval=0.1)))
            except psutil.NoSuchProcess:
                pass
        else:
            for proc in psutil.process_iter(['pid', 'name', 'cpu_percent']):
                cpu_usage_data.append((proc.info['pid'], proc.info['name'], proc.info['cpu_percent']))
        return cpu_usage_data

    def update_display(self):
        '''Updates the GUI with the latest CPU usage data.'''
        for row in self.tree.get_children():
            self.tree.delete(row)
        cpu_usage_data = self.get_cpu_usage()
        for pid, name, cpu_usage in cpu_usage_data:
            self.tree.insert('', tk.END, values=(pid, name, cpu_usage))
        self.root.after(self.update_interval * 1000, self.update_display)

def main():
    pid = None  # Replace with a specific PID if needed
    root = tk.Tk()
    app = CPUMonitorApp(root, pid)
    root.mainloop()

if __name__ == '__main__':
    main()
