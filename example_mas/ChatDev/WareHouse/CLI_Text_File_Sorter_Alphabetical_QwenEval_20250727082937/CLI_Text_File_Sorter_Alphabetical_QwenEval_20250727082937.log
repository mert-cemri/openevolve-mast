class FileHandler:
    def read_file(self, file_path):
        '''
        Reads the contents of a file and returns the lines as a list.
        Parameters:
        file_path (str): The path to the file to be read.
        Returns:
        list of str: The lines of the file.
        '''
        with open(file_path, 'r') as file:
