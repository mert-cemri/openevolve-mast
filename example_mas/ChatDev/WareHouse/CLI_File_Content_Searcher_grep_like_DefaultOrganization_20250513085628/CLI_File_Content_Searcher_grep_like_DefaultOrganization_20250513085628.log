'''

Main application file for the CLI program with a GUI that searches for a given string/pattern within all text files in a specified directory and its subdirectories.

'''

import os
import tkinter as tk
from tkinter import filedialog, messagebox
from search import search_in_files

class SearchApp:
    def __init__(self):
        self.root = tk.Tk()
        self.root.title("Text File Searcher")
        
        self.directory = tk.StringVar()
        self.pattern = tk.StringVar()
        
        self.create_widgets()
        
    def create_widgets(self):
        tk.Label(self.root, text="Directory:").grid(row=0, column=0, padx=5, pady=5, sticky='e')
        tk.Entry(self.root, textvariable=self.directory, width=50).grid(row=0, column=1, padx=5, pady=5)
        tk.Button(self.root, text="Browse", command=self.browse_directory).grid(row=0, column=2, padx=5, pady=5)
        
        tk.Label(self.root, text="Pattern:").grid(row=1, column=0, padx=5, pady=5, sticky='e')
        tk.Entry(self.root, textvariable=self.pattern, width=50).grid(row=1, column=1, padx=5, pady=5)
        
        tk.Button(self.root, text="Search", command=self.perform_search).grid(row=2, column=1, pady=10)
        
    def browse_directory(self):
        directory = filedialog.askdirectory()
        if directory:
            self.directory.set(directory)
    
    def perform_search(self):
        directory = self.directory.get()
        pattern = self.pattern.get()
        
        if not directory or not pattern:
            messagebox.showwarning("Input Error", "Please specify both directory and pattern.")
            return
        
        results = search_in_files(directory, pattern)
        if results:
            result_text = "\n".join([f"{file}: {line}" for file, line in results])
            messagebox.showinfo("Search Results", result_text)
        else:
            messagebox.showinfo("Search Results", "No matches found.")
    
    def run(self):
        self.root.mainloop()

if __name__ == "__main__":
    app = SearchApp()
    app.run()
```

