    else:
        headers = flat_data[0].keys() if flat_data else []
        filtered_data = flat_data
    with open(output_path, 'w', newline='') as f:
        writer = csv.DictWriter(f, fieldnames=headers)
        writer.writeheader()
        writer.writerows(filtered_data)
