import tkinter as tk
from tkinter import filedialog, messagebox
from filesearcher import FileSearcher

class SearchApp:
    def __init__(self, root):
        '''
        Initializes the GUI components.
        '''
        self.root = root
        self.root.title("Text File Searcher")
        
        self.directory = tk.StringVar()
        self.pattern = tk.StringVar()
        
        tk.Label(root, text="Directory:").grid(row=0, column=0, padx=5, pady=5)
        tk.Entry(root, textvariable=self.directory, width=50).grid(row=0, column=1, padx=5, pady=5)
        tk.Button(root, text="Browse", command=self.browse_directory).grid(row=0, column=2, padx=5, pady=5)
        
        tk.Label(root, text="Pattern:").grid(row=1, column=0, padx=5, pady=5)
        tk.Entry(root, textvariable=self.pattern, width=50).grid(row=1, column=1, padx=5, pady=5)
        
        tk.Button(root, text="Search", command=self.perform_search).grid(row=2, column=1, pady=10)
        
        self.results_text = tk.Text(root, width=80, height=20)
        self.results_text.grid(row=3, column=0, columnspan=3, padx=5, pady=5)
    
    def browse_directory(self):
        '''
        Opens a dialog to select a directory.
        '''
        directory = filedialog.askdirectory()
        if directory:
            self.directory.set(directory)
    
    def perform_search(self):
        '''
        Initiates the search using the FileSearcher class.
        '''
        directory = self.directory.get()
        pattern = self.pattern.get()
        
        if not directory or not pattern:
            messagebox.showwarning("Input Error", "Please specify both directory and pattern.")
            return
        
        searcher = FileSearcher()
        results = searcher.search_files(directory, pattern)
        self.display_results(results)
    
    def display_results(self, results):
        '''
        Displays the search results in the GUI.
        '''
        self.results_text.delete(1.0, tk.END)
        for filename, lines in results.items():
            self.results_text.insert(tk.END, f"File: {filename}\n")
            for line in lines:
                self.results_text.insert(tk.END, f"  {line}\n")
            self.results_text.insert(tk.END, "\n")

def main():
    '''
    Entry point of the application. Sets up the GUI and starts the main loop.
    '''
    root = tk.Tk()
    app = SearchApp(root)
    root.mainloop()

if __name__ == "__main__":
    main()
```

