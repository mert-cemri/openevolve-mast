        tk.Button(self.root, text="Resize", command=self.resize_image).grid(row=4, column=1, padx=10, pady=10)

    def browse_input(self):
        file_path = filedialog.askopenfilename(filetypes=[("Image files", "*.jpg *.jpeg *.png *.bmp *.gif")])
        self.input_path.set(file_path)

    def browse_output(self):
        file_path = filedialog.asksaveasfilename(defaultextension=".jpg", filetypes=[("JPEG files", "*.jpg"), ("PNG files", "*.png"), ("BMP files", "*.bmp"), ("GIF files", "*.gif")])
        self.output_path.set(file_path)

    def resize_image(self):
        input_path = self.input_path.get()
        output_path = self.output_path.get()
        target_width = self.target_width.get()
        target_height = self.target_height.get()

        if not input_path or not output_path:
            messagebox.showerror("Error", "Please specify both input and output image paths.")
            return

        if target_width and target_height:
            messagebox.showerror("Error", "Only one of target width or height should be specified.")
            return

        try:
            target_width = int(target_width) if target_width else None
            target_height = int(target_height) if target_height else None
        except ValueError:
            messagebox.showerror("Error", "Target width and height should be integers.")
            return

        resizer = ImageResizer(input_path, output_path, target_width, target_height)
        resizer.resize_image()
        messagebox.showinfo("Success", f"Image resized and saved to {resizer.get_output_path()}")

    def run(self):
        self.root.mainloop()
