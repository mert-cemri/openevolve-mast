'''
Plotter class to generate text-based bar charts and line graphs.
'''
class Plotter:
    def __init__(self, data):
        self.data = data

    def plot_bar_chart(self):
        print("\nBar Chart:")
        max_value = max(self.data) if self.data else 0
        scale = 50 / max_value if max_value > 0 else 1
        bar_chart = []
        for value in self.data:
            bar = '#' * int(value * scale)
            bar_chart.append(f"{value:6.2f} | {bar}")
        print("\n".join(bar_chart))
        return "\n".join(bar_chart)

    def plot_line_graph(self):
        print("\nLine Graph:")
        max_value = max(self.data) if self.data else 0
        scale = 50 / max_value if max_value > 0 else 1
        line_graph = []
        for i, value in enumerate(self.data):
            if i > 0:
                prev_scaled = int(self.data[i-1] * scale)
                curr_scaled = int(value * scale)
                # Ensure the line is always visible by adding a minimum length
                if abs(curr_scaled - prev_scaled) < 2:
                    line = ' ' * prev_scaled + '*' + ' ' * (curr_scaled - prev_scaled) + '*'
                    if curr_scaled > prev_scaled:
                        line += ' ' * 1 + '*'
                    else:
                        line = ' ' * 1 + '*' + line
                else:
                    line = ' ' * prev_scaled + '*' + ' ' * (curr_scaled - prev_scaled) + '*'
            else:
                line = '*' * int(value * scale)
            line_graph.append(f"{value:6.2f} | {line}")
        print("\n".join(line_graph))
        return "\n".join(line_graph)
