class FileHandler:
    def __init__(self, filename='vault.dat'):
        self.filename = filename

    def read_file(self):
        try:
            with open(self.filename, 'rb') as file:
                return file.read()
        except FileNotFoundError:
            return None

    def write_file(self, data):
        with open(self.filename, 'wb') as file:
