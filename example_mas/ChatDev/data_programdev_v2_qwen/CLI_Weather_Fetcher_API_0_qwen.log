'''
Handles the GUI application logic.
Fetches and displays weather data for a given city.
'''

import tkinter as tk
from tkinter import messagebox
import requests

class WeatherFetcher:
    def __init__(self, api_key):
        self.api_key = api_key
        self.base_url = "http://api.openweathermap.org/data/2.5/weather?"

    def fetch_weather(self, city_name):
        complete_url = f"{self.base_url}appid={self.api_key}&q={city_name}&units=metric"
        response = requests.get(complete_url)
        return response.json()

    def parse_weather_data(self, data):
        if data["cod"] != 404:
            main_data = data["main"]
            temperature = main_data["temp"]
            pressure = main_data["pressure"]
            humidity = main_data["humidity"]
            weather_description = data["weather"][0]["description"]
            return f"Temperature: {temperature}C\nPressure: {pressure} hPa\nHumidity: {humidity}%\nDescription: {weather_description}"
        else:
            return "City Not Found"

class WeatherApp:
    def __init__(self):
        self.api_key = "YOUR_API_KEY"  # Replace with your OpenWeatherMap API key
        self.weather_fetcher = WeatherFetcher(self.api_key)
        self.root = tk.Tk()
        self.root.title("Weather App")
        self.root.geometry("300x200")

        self.city_label = tk.Label(self.root, text="Enter City:")
        self.city_label.pack(pady=10)

        self.city_entry = tk.Entry(self.root)
        self.city_entry.pack(pady=5)

        self.get_weather_button = tk.Button(self.root, text="Get Weather", command=self.get_weather)
        self.get_weather_button.pack(pady=10)

        self.weather_label = tk.Label(self.root, text="", justify="left")
        self.weather_label.pack(pady=10)

    def run(self):
        self.root.mainloop()

    def get_weather(self):
        city_name = self.city_entry.get()
        if city_name:
            weather_data = self.weather_fetcher.fetch_weather(city_name)
            parsed_data = self.weather_fetcher.parse_weather_data(weather_data)
            self.display_weather(parsed_data)
        else:
            messagebox.showwarning("Input Error", "Please enter a city name.")

    def display_weather(self, weather_data):
        self.weather_label.config(text=weather_data)

```

### Step 3: Running the Application

To run the application, you need to replace `"YOUR_API_KEY"` in `weather_app.py` with your actual OpenWeatherMap API key. Then, simply execute `main.py`:

```bash
python main.py
```

This will open a window where you can enter a city name and fetch the current weather data for that city. The weather data will be displayed in the GUI.

