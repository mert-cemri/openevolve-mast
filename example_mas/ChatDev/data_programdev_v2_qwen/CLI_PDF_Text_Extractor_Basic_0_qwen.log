import pdftotext
class PDFExtractor:
    def extract_text(self, pdf_path):
        '''
        Extracts text from a PDF file and returns it as a string.
        '''
        try:
            with open(pdf_path, "rb") as f:
                pdf = pdftotext.PDF(f)
            return "\n\n".join(pdf)
        except Exception as e:
            raise Exception(f"Failed to extract text from PDF: {e}")
