        self.count_entry.pack(pady=5)

        self.ping_button = tk.Button(root, text="Ping", command=self.start_ping)
        self.ping_button.pack(pady=10)

        self.results_text = tk.Text(root, width=40, height=10)
        self.results_text.pack(pady=10)

    def start_ping(self):
        host = self.host_entry.get()
        count = self.count_entry.get()
