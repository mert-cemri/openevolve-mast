```python
'''

Main file for the CLI dictionary lookup tool with GUI.
This file initializes the main application and starts the GUI loop.

'''

import tkinter as tk
from tkinter import messagebox
from dictionary import Dictionary

class DictionaryApp:
    def __init__(self, root):
        self.root = root
        self.root.title("Dictionary Lookup Tool")
        
        self.dictionary = Dictionary("dictionary.txt")
        
        self.create_widgets()
    
    def create_widgets(self):
        self.label = tk.Label(self.root, text="Enter a word:")
        self.label.pack(pady=10)
        
        self.entry = tk.Entry(self.root, width=40)
        self.entry.pack(pady=5)
        
        self.search_button = tk.Button(self.root, text="Search", command=self.search_word)
        self.search_button.pack(pady=5)
        
        self.result_label = tk.Label(self.root, text="", wraplength=300, justify="left")
        self.result_label.pack(pady=10)
    
    def search_word(self):
        word = self.entry.get().strip()
        if not word:
            messagebox.showwarning("Input Error", "Please enter a word.")
            return
        
        definition = self.dictionary.get_definition(word)
        if definition:
            self.display_definition(definition)
        else:
            messagebox.showinfo("Not Found", f"No definition found for '{word}'.")
    
    def display_definition(self, definition):
        self.result_label.config(text=definition)

if __name__ == "__main__":
    root = tk.Tk()
    app = DictionaryApp(root)
    root.mainloop()
```

