'''
Module for generating QR codes from text or URLs.
Includes functions to generate, save, and convert QR codes to ASCII art.
'''

import qrcode
from PIL import Image

def generate_qr_code(data):
    qr = qrcode.QRCode(
        version=1,
        error_correction=qrcode.constants.ERROR_CORRECT_L,
        box_size=10,
        border=4,
    )
    qr.add_data(data)
    qr.make(fit=True)
    img = qr.make_image(fill_color="black", back_color="white")
    return img

def save_qr_code(qr_code, filename):
    qr_code.save(filename)

def qr_to_ascii(qr_code):
    img = qr_code.convert('L')
    pixels = img.getdata()
    ascii_chars = ['@', '#', 'S', '%', '?', '*', '+', ';', ':', ',', '.']
    ascii_str = ''
    for pixel_value in pixels:
        ascii_str += ascii_chars[pixel_value // 25]
    img_width = img.width
    ascii_str_len = len(ascii_str)
    ascii_img = '\n'.join([ascii_str[index:(index + img_width)] for index in range(0, ascii_str_len, img_width)])
    return ascii_img
```

