'''
Main module for the QR Code Generator CLI tool with GUI.
Handles command-line arguments and starts the GUI.
'''

import argparse
from gui import QRCodeApp

def parse_arguments():
    parser = argparse.ArgumentParser(description='QR Code Generator CLI tool with GUI')
    parser.add_argument('--text', type=str, help='Text or URL to generate QR code from')
    parser.add_argument('--output', type=str, help='Output file to save the QR code image')
    return parser.parse_args()

def main():
