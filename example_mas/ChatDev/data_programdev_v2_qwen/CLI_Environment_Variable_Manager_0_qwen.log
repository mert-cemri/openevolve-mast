            with open(self.profile_path, "w") as file:
                pass
        with open(self.profile_path, "a") as file:
            if 'fish' in self.profile_path:
                file.write(f'set -x {name} "{value}"\n')
            else:
                file.write(f'export {name}="{value}"\n')
