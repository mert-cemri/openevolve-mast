'''
Regex engine module that handles regex matching.
'''

import re

def match_regex(regex, test_string):
    try:
        pattern = re.compile(regex)
        match = pattern.match(test_string)
        return match
    except re.error as e:
        print(f"Regex error: {e}")
        return None
