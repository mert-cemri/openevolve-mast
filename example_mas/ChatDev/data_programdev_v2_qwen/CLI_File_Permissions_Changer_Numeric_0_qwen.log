import argparse
import sys
from gui import App

def parse_arguments():
    parser = argparse.ArgumentParser(description='Change file permissions using numeric (octal) notation.')
    parser.add_argument('-g', '--gui', action='store_true', help='Run the GUI version of the utility')
    parser.add_argument('mode', type=str, nargs='?', help='Numeric (octal) permission mode (e.g., 755, 644)')
    parser.add_argument('path', type=str, nargs='?', help='File or directory path')
    return parser.parse_args()

