        self.update_uptime()

    def update_uptime(self):
        uptime = self.uptime_fetcher.get_uptime()
        self.label.config(text=f"System Uptime: {uptime}")
        self.root.after(1000, self.update_uptime)

    def display_uptime(self):
        self.root.mainloop()
