class GUIInterface:
    def __init__(self, uptime_fetcher):
        self.uptime_fetcher = uptime_fetcher
        self.root = tk.Tk()
        self.root.title("System Uptime")
        self.root.geometry("300x100")

        self.label = ttk.Label(self.root, text="Fetching Uptime...", font=("Helvetica", 14))
