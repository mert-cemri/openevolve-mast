    def save_recipes(self, recipes):
        '''
        Saves recipes to the JSON file.
        :param recipes: List of Recipe objects to save.
        '''
        with open(self.filename, 'w') as file:
            json.dump([recipe.__dict__ for recipe in recipes], file, indent=4)
