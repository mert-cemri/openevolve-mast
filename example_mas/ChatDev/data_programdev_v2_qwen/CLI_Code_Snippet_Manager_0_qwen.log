import json
from file_handler import FileHandler

class Snippet:
    def __init__(self, language, code, description=""):
        self.language = language
        self.code = code
        self.description = description

    def to_dict(self):
        return {
            "language": self.language,
            "code": self.code,
            "description": self.description
        }

class SnippetManager:
