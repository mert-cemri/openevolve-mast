import sys
import converter
import config
import gui

def main():
    if len(sys.argv) == 4:
        try:
            amount = float(sys.argv[1])
            source_currency = sys.argv[2].upper()
            target_currency = sys.argv[3].upper()
            result = converter.convert(amount, source_currency, target_currency)
            print(f"{amount} {source_currency} is {result} {target_currency}")
        except ValueError:
            print("Invalid input. Please provide a valid amount and currency codes.")
    else:
        print("Usage: python main.py <amount> <source_currency> <target_currency>")
        print("Example: python main.py 100 USD EUR")

def run_gui():
    app = gui.CurrencyConverterApp()
    app.mainloop()

if __name__ == "__main__":
    if len(sys.argv) > 1:
        main()
    else:
        run_gui()
```

