```python
'''
Handles the currency conversion logic.
'''

import config

def convert(amount, source_currency, target_currency):
    exchange_rates = config.load_exchange_rates()
    if source_currency not in exchange_rates or target_currency not in exchange_rates:
        raise ValueError("Invalid currency code.")
    base_rate = exchange_rates[source_currency]
    target_rate = exchange_rates[target_currency]
    converted_amount = (amount / base_rate) * target_rate
    return converted_amount
