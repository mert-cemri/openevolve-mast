        return False
    def draw(self, screen):
        for y, row in enumerate(self.shape):
            for x, cell in enumerate(row):
                if cell:
                    pygame.draw.rect(screen, WHITE, ((self.position[1] + x) * BLOCK_SIZE, (self.position[0] + y) * BLOCK_SIZE, BLOCK_SIZE, BLOCK_SIZE))
