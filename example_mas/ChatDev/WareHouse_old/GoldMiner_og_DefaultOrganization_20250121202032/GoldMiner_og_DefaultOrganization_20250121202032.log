        if self.level.check_completion(self.claw, self.objects):
            print("Level Complete!")
            pygame.quit()
            exit()

    def draw(self):
        self.screen.fill((0, 0, 0))
        self.claw.draw(self.screen)
        for obj in self.objects:
            obj.draw(self.screen)
        pygame.display.flip()

if __name__ == "__main__":
    game = Game()
    game.run()
