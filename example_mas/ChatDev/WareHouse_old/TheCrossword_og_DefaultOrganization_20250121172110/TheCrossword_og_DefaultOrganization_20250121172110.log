import tkinter as tk
from tkinter import messagebox
from crossword import CrosswordGrid, Clue

class CrosswordApp:
    def __init__(self, master):
        self.master = master
        self.master.title("Crossword Puzzle")
        self.grid_size = 10
        self.crossword = CrosswordGrid(self.grid_size)
        self.create_widgets()

    def create_widgets(self):
        self.entries = [[None for _ in range(self.grid_size)] for _ in range(self.grid_size)]
        for r in range(self.grid_size):
            for c in range(self.grid_size):
                entry = tk.Entry(self.master, width=2, font=('Arial', 18), justify='center')
                entry.grid(row=r, column=c)
                self.entries[r][c] = entry

        self.check_button = tk.Button(self.master, text="Check", command=self.check_solution)
        self.check_button.grid(row=self.grid_size, column=0, columnspan=self.grid_size)

        self.add_clues()

    def add_clues(self):
        # Example clues
        self.crossword.add_clue(Clue(1, "First letter of the alphabet", "A", 0, 0, 'across'))
        self.crossword.add_clue(Clue(2, "Opposite of down", "UP", 1, 0, 'across'))
        self.crossword.add_clue(Clue(3, "Not in", "OUT", 0, 0, 'down'))

    def check_solution(self):
        for clue in self.crossword.clues:
            word = ''
            row, col = clue.start_row, clue.start_col
            for i in range(len(clue.answer)):
                if clue.direction == 'across':
                    word += self.entries[row][col + i].get()
                elif clue.direction == 'down':
                    word += self.entries[row + i][col].get()
            if not self.crossword.validate_word(clue.number, clue.direction, word):
                messagebox.showerror("Error", f"Incorrect word for clue {clue.number} {clue.direction}")
                return
        if self.crossword.is_complete():
            messagebox.showinfo("Success", "Congratulations! You completed the crossword.")
        else:
            messagebox.showerror("Error", "The crossword is not complete yet.")

def main():
    root = tk.Tk()
    app = CrosswordApp(root)
    root.mainloop()

if __name__ == "__main__":
    main()
```

