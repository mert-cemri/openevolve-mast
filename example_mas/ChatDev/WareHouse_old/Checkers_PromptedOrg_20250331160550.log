                if piece is not None:
                    if piece.color == 'W':
                        white_pieces += 1
                    else:
                        black_pieces += 1
        return white_pieces == 0 or black_pieces == 0

    def is_within_bounds(self, pos):
        return 0 <= pos[0] < 8 and 0 <= pos[1] < 8
