            elif row_diff == 2 * direction and abs(row_diff) == 2:
                mid_row = (from_pos[0] + to_pos[0]) // 2
                mid_col = (from_pos[1] + to_pos[1]) // 2
                mid_piece = self.grid[mid_row][mid_col]
                if mid_piece is not None and mid_piece.color != player:
