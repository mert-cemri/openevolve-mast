                            # Check for capture moves
                            to_pos = (row + 2*d_row, col + 2*d_col)
                            if self.is_valid_move((row, col), to_pos, current_player):
                                return False
        return True
    def is_within_bounds(self, pos):
        return 0 <= pos[0] < 8 and 0 <= pos[1] < 8
