    def find_matches(self):
        matches = []
        # Check horizontal matches
        for row in range(self.rows):
            for col in range(self.cols - 2):
                if self.grid[row][col].color == self.grid[row][col + 1].color == self.grid[row][col + 2].color:
                    matches.append((row, col))
