import random
class HangmanGame:
    def __init__(self, word_list):
        self.word_list = word_list
        self.word = ""
        self.guessed_letters = set()
        self.attempts_left = 6
    def select_random_word(self):
        self.word = random.choice(self.word_list)
        self.guessed_letters.clear()
        self.attempts_left = 6
