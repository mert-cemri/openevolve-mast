'''

Defines the SudokuBoard class, which represents the Sudoku grid and contains methods for board manipulation and validation.

'''

class SudokuBoard:
    def __init__(self, board):
        self.board = board

    def is_valid_move(self, row, col, num):
        # Check if the number is not in the current row, column, and 3x3 subgrid
        for i in range(9):
            if self.board[row][i] == num or self.board[i][col] == num:
                return False
        
        start_row, start_col = 3 * (row // 3), 3 * (col // 3)
        for i in range(3):
            for j in range(3):
                if self.board[start_row + i][start_col + j] == num:
                    return False
        
        return True

    def is_complete(self):
        # Check if all cells are filled correctly
        for row in range(9):
            for col in range(9):
                num = self.board[row][col]
                if num == 0 or not self.is_valid_move(row, col, num):
                    return False
        return True

    def place_number(self, row, col, num):
        if self.is_valid_move(row, col, num):
            self.board[row][col] = num
            return True
        return False
```

