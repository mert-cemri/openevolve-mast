        # Select or deselect a word
        if word in self.selected_words:
            self.selected_words.remove(word)
            self.buttons[word].config(relief=tk.RAISED)
        else:
            self.selected_words.append(word)
            self.buttons[word].config(relief=tk.SUNKEN)

