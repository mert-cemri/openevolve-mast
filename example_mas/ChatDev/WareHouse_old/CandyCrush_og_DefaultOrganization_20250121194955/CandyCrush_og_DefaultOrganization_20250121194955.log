    def find_matches(self):
        self.matches.clear()
        for row in range(self.rows):
            for col in range(self.cols):
                # Check for horizontal matches
                match_length = 1
                while col + match_length < self.cols and self.grid[row][col].type == self.grid[row][col + match_length].type:
                    match_length += 1
                if match_length >= 3:
