        self.canvas.create_text(300, 300, text=f"{winner} wins!", font=("Arial", 24), fill="red")
    def show_draw(self):
        self.canvas.unbind("<Button-1>")
        self.canvas.create_text(300, 300, text="It's a draw!", font=("Arial", 24), fill="blue")
