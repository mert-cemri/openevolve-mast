                elif event.key == pygame.K_RIGHT:
                    self.snake.change_direction('RIGHT')
    def update(self):
        self.snake.move()
        if self.snake.check_collision():
            self.running = False
        if self.snake.head == self.food.position:
            self.snake.grow()
