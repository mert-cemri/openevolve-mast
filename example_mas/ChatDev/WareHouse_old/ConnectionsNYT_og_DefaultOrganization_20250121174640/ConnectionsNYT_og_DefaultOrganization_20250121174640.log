            "Animals": ["Dog", "Cat", "Elephant", "Lion"],
            "Colors": ["Red", "Blue", "Green", "Yellow"],
            "Countries": ["USA", "Canada", "Mexico", "Brazil"]
        }
        self.words = [word for category in self.categories.values() for word in category]
        self.correct_groups = []
        self.max_tries = 10
        self.tries = 0
