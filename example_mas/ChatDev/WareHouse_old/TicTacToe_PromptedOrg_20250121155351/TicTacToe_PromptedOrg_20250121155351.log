                row, col = map(int, move.split(","))
                if 0 <= row < 3 and 0 <= col < 3:
                    return row, col
                else:
                    print("Invalid input. Please enter numbers between 0 and 2.")
            except ValueError:
                print("Invalid input. Please enter numbers in the format 'row,col'.")
