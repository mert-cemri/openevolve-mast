        elif self.direction == pygame.K_DOWN:
            head_y += 1
        elif self.direction == pygame.K_LEFT:
            head_x -= 1
        elif self.direction == pygame.K_RIGHT:
            head_x += 1
        new_head = (head_x, head_y)
        self.body.appendleft(new_head)
        if not self.growing:
            self.body.pop()
        else:
