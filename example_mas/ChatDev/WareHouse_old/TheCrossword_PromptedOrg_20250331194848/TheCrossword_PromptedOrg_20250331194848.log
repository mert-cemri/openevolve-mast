                3: ('WORLD', (2, 0))
            },
            'down': {
                2: ('EARTH', (0, 2)),
                4: ('LOW', (0, 4))
            }
        }

        # Clues for across and down
        self.clues = {
            'across': {
                1: 'Greeting word',
                3: 'Our planet'
            },
            'down': {
                2: 'Third planet from the sun',
                4: 'Opposite of high'
            }
        }

