class CrosswordGrid:
    def __init__(self, rows, cols):
        self.rows = rows
        self.cols = cols
        self.grid = [['' for _ in range(cols)] for _ in range(rows)]

    def add_word(self, clue, word):
        if clue.direction == 'across':
            for j, letter in enumerate(word):
                self.grid[clue.number - 1][j] = letter
        elif clue.direction == 'down':
            for i, letter in enumerate(word):
                self.grid[i][clue.number - 1] = letter

    def validate_word(self, clue, word):
        if clue.answer == word:
            self.add_word(clue, word)
            return True
        return False

    def is_complete(self):
        for row in self.grid:
            if '' in row:
                return False
        return True

class Clue:
    def __init__(self, number, direction, text, answer):
        self.number = number
        self.direction = direction
        self.text = text
        self.answer = answer
