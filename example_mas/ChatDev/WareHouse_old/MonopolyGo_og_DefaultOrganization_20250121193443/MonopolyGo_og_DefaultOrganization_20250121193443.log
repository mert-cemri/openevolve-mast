        self.info_label.config(text=info)
        self.root.update()

    def prompt_player_action(self, player):
        self.update_display()

    def roll_dice(self):
        current_player = self.game.players[self.game.current_player_index]
        self.game.roll_dice_and_move(current_player)

    def start(self):
        self.root.mainloop()
