import random
from monster import Monster
from treasure_chest import TreasureChest
from door import Door

class Floor:
    pass

class Map:
    def __init__(self):
        self.width = 80
        self.height = 80
        self.grid = [[Floor() for _ in range(self.width)] for _ in range(self.height)]
        self.start_position = (0, 0)

    def generate_map(self):
        # Simplified map generation for demonstration
        self.grid[0][0] = 'Start'
        self.grid[79][79] = Door()
        # Ensure at least one path exists from start to door
        self.create_path(self.start_position, (79, 79))
        # Add random monsters and treasure chests
        for _ in range(10):
            x, y = random.randint(0, 79), random.randint(0, 79)
            self.grid[x][y] = Monster()
        for _ in range(5):
            x, y = random.randint(0, 79), random.randint(0, 79)
            self.grid[x][y] = TreasureChest()

    def create_path(self, start, end):
        # Implement a pathfinding algorithm like A* or Dijkstra's to create a path
        # For now, we'll assume a simple direct path for demonstration
        x, y = start
        while x < end[0]:
            x += 1
            self.grid[x][y] = Floor()
        while y < end[1]:
            y += 1
            self.grid[x][y] = Floor()

    def is_walkable(self, x, y):
        return 0 <= x < self.width and 0 <= y < self.height and (isinstance(self.grid[x][y], Floor) or self.grid[x][y] == 'Start')

    def get_object_at(self, position):
        x, y = position
        return self.grid[x][y]

    def display(self, player_position):
        for y in range(self.height):
            for x in range(self.width):
                if (x, y) == player_position:
                    print('P', end='')
                elif isinstance(self.grid[x][y], Monster):
                    print('M', end='')
                elif isinstance(self.grid[x][y], TreasureChest):
                    print('T', end='')
                elif isinstance(self.grid[x][y], Door):
                    print('D', end='')
                else:
                    print('.', end='')
            print()
