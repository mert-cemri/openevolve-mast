        self.top_rect.x -= 5
        self.bottom_rect.x -= 5

    def draw(self, screen):
        screen.blit(self.image, self.top_rect)
        screen.blit(self.image, self.bottom_rect)

    def is_off_screen(self):
        return self.top_rect.right < 0
