'''
Main file to run the Checkers game. Handles user interaction and game loop.
'''

from board import Board

def parse_input(move_str):
    try:
        from_str, to_str = move_str.strip().split('-')
        from_pos = tuple(map(int, from_str.split(',')))
        to_pos = tuple(map(int, to_str.split(',')))
        return from_pos, to_pos
    except:
        return None, None

def main():
    board = Board()
    current_player = 'W'
    player_names = {'W': 'White', 'B': 'Black'}

    while True:
        board.draw_board()
        winner = board.check_winner()
        if winner:
            print(f"{winner} wins!")
            break

        print(f"{player_names[current_player]}'s turn.")
        move_str = input("Enter move (from_row,from_col-to_row,to_col): ")
        from_pos, to_pos = parse_input(move_str)

        if from_pos is None or not board.valid_move(from_pos, to_pos, current_player):
            print("Invalid move. Try again.")
            continue

        board.move_piece(from_pos, to_pos)
        current_player = 'B' if current_player == 'W' else 'W'

if __name__ == "__main__":
    main()
```

