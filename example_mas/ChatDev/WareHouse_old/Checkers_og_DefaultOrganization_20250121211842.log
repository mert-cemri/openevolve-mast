                    if 0 <= jump_row < 8 and 0 <= jump_col < 8 and self.board.grid[jump_row][jump_col] is None:
                        moves.append((jump_row, jump_col))
        return moves

    def switch_turn(self):
        self.current_turn = 1 - self.current_turn
