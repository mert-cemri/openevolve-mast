        for i in range(80):
            map[0][i] = 'wall'
            map[79][i] = 'wall'
            map[i][0] = 'wall'
            map[i][79] = 'wall'
        # Place a door
        map[78][78] = Door((78, 78))
        # Place some monsters
        map[10][10] = Monster((10, 10), 20)
        map[20][20] = Monster((20, 20), 30)
        # Place some treasure chests
        map[15][15] = TreasureChest((15, 15))
        map[25][25] = TreasureChest((25, 25))
        return map

    def is_walkable(self, x, y):
        # Check if the position is within bounds and not a wall
        return 0 <= x < 80 and 0 <= y < 80 and self.map[y][x] is None

    def get_object_at(self, x, y):
        return self.map[y][x]

