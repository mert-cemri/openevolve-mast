        elif direction == 'left' and game_map.is_walkable(x - 1, y):
            self.position = (x - 1, y)
        elif direction == 'right' and game_map.is_walkable(x + 1, y):
            self.position = (x + 1, y)

    def draw(self, screen):
        screen.blit(self.image, (self.position[0] * 10, self.position[1] * 10))
