import pygame
import random

class GameMap:
    def __init__(self):
        self.width = 80
        self.height = 80
        self.map = self.generate_map()
        self.start_position = (0, 0)  # Example starting position

    def generate_map(self):
        # Simplified map generation for demonstration
        return [[random.choice(['floor', 'wall']) for _ in range(self.width)] for _ in range(self.height)]

    def is_walkable(self, x, y):
        if 0 <= x < self.width and 0 <= y < self.height:
            return self.map[y][x] == 'floor'
        return False

    def draw(self, screen):
        for y, row in enumerate(self.map):
            for x, tile in enumerate(row):
                color = (255, 255, 255) if tile == 'floor' else (0, 0, 0)
                pygame.draw.rect(screen, color, pygame.Rect(x * 10, y * 10, 10, 10))
