def load_words():
    with open("words.txt", "r") as file:
        words = file.read().splitlines()
    return words
def calculate_wpm(typed_words, elapsed_time):
    word_count = len(typed_words)
    minutes = elapsed_time / 60
    wpm = word_count / minutes
    return round(wpm, 2)
