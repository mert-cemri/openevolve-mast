        self.draw_board()
    def draw_board(self):
        self.canvas.delete("all")
        for i in range(self.game.size):
            self.canvas.create_line(40, 40 + i * 40, 40 + (self.game.size - 1) * 40, 40 + i * 40)
            self.canvas.create_line(40 + i * 40, 40, 40 + i * 40, 40 + (self.game.size - 1) * 40)
