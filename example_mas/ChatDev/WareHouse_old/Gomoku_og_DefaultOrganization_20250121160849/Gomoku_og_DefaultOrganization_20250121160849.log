            if result:
                self.status_label.config(text=result)
                self.prompt_new_game()
            else:
                self.update_status()
            self.draw_board()
            self.draw_pieces()
    def draw_pieces(self):
        for x in range(self.game.size):
            for y in range(self.game.size):
