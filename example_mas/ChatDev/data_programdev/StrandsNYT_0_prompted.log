                return False
            for i in range(len(word)):
                if self.grid[start_row + i][start_col] not in ('', word[i]):
                    return False
        elif direction == 'diagonal':
            if start_row + len(word) > 6 or start_col + len(word) > 8:
                return False
            for i in range(len(word)):
                if self.grid[start_row + i][start_col + i] not in ('', word[i]):
                    return False
        return True

