import pygame
class Pipe:
    def __init__(self, x, gap_y, gap_size, speed):
        self.x = x
        self.gap_y = gap_y
        self.gap_size = gap_size
        self.speed = speed
        self.width = 60
        self.color = (34, 139, 34)  # Green pipes
        self.passed = False
