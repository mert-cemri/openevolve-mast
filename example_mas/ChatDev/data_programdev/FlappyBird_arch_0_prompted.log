class Game:
    def __init__(self, screen, clock):
        self.screen = screen
        self.clock = clock
        self.bird = Bird(100, 300)
        self.pipes = []
        self.spawn_timer = 0
        self.score = 0
        self.font = pygame.font.SysFont(None, 48)
        self.running = True
        self.pipe_speed = 4
        self.pipe_gap = 200
    def reset(self):
        self.bird = Bird(100, 300)
        self.pipes.clear()
        self.spawn_timer = 0
        self.score = 0
        self.pipe_speed = 4
        self.pipe_gap = 200
    def handle_events(self):
        for event in pygame.event.get():
            if event.type == pygame.QUIT:
                self.running = False
            if event.type == pygame.KEYDOWN:
                if event.key == pygame.K_SPACE:
                    self.bird.flap()
    def update(self):
        self.bird.update()
        self.spawn_timer += 1
        if self.spawn_timer > 90:
            self.spawn_timer = 0
            gap_y = random.randint(150, 450)
            self.pipes.append(Pipe(400, gap_y, self.pipe_gap, self.pipe_speed))
        for pipe in self.pipes:
            pipe.update()
            if pipe.collide(self.bird.get_rect()) or not self.screen.get_rect().contains(self.bird.get_rect()):
                self.running = False  # Temporarily stop the game loop
                self.draw()  # Draw the final frame
                pygame.display.flip()
                pygame.time.wait(1000)  # Pause for 1 second to indicate collision clearly
                self.reset()
                self.running = True  # Restart the game loop
                break  # Break the loop to avoid further processing after collision
            if pipe.passed is False and pipe.x + pipe.width < self.bird.x:
                pipe.passed = True
                self.score += 1
                # Increase difficulty gradually
                if self.score % 5 == 0:
                    self.pipe_speed += 0.5
                    self.pipe_gap = max(120, self.pipe_gap - 10)
        self.pipes = [pipe for pipe in self.pipes if not pipe.off_screen()]
    def draw(self):
        self.screen.fill((135, 206, 235))  # Sky blue background
        self.bird.draw(self.screen)
        for pipe in self.pipes:
            pipe.draw(self.screen)
        score_img = self.font.render(str(self.score), True, (255, 255, 255))
        self.screen.blit(score_img, (200 - score_img.get_width() // 2, 20))
        pygame.display.flip()
    def run(self):
        while self.running:
            self.handle_events()
            self.update()
            self.draw()
            self.clock.tick(60)
