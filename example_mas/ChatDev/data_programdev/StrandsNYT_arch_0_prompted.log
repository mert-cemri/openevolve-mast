'''
Game class handles the main game logic, user interaction, scoring, and hints.
'''

class Game:
    def __init__(self, puzzle_board, theme_words, spangram, non_theme_words):
        self.board = puzzle_board
        self.theme_words = theme_words
        self.spangram = spangram
        self.non_theme_words = non_theme_words
        self.found_theme_words = []
        self.found_non_theme_words = []
        self.hints_unlocked = 0

    def check_word(self, word, path):
        if not self.board.is_valid_word(word, path):
            return "Invalid word or path."

        if word == self.spangram:
            self.found_theme_words.append(word)
            return "Spangram found! Highlighted in yellow."

        if word in self.theme_words and word not in self.found_theme_words:
            self.found_theme_words.append(word)
            return "Theme word found! Highlighted in blue."

        if word in self.non_theme_words and word not in self.found_non_theme_words:
            self.found_non_theme_words.append(word)
            if len(self.found_non_theme_words) % 3 == 0:
                self.unlock_hint()
                return "Non-theme word found! Hint unlocked!"
            return "Non-theme word found!"

        return "Word already found or not recognized."

    def unlock_hint(self):
        self.hints_unlocked += 1
        print(f"Hint #{self.hints_unlocked} unlocked! Try looking near corners or edges.")

    def play(self):
        print("Welcome to NYT Strands Puzzle!")
        while len(self.found_theme_words) < len(self.theme_words) + 1:  # +1 for spangram
            self.board.display_board(
                [(word, [(x,y) for y in range(self.board.height) for x in range(self.board.width) if self.board.board[y][x] in word], 'blue') for word in self.found_theme_words if word != self.spangram] +
                [(self.spangram, [(x,y) for y in range(self.board.height) for x in range(self.board.width) if self.board.board[y][x] in self.spangram], 'yellow')] if self.spangram in self.found_theme_words else []
            )
            word = input("Enter word found: ").upper()
            path_input = input("Enter path as x1,y1;x2,y2;...: ")
            path = [tuple(map(int, p.split(','))) for p in path_input.split(';')]
            result = self.check_word(word, path)
            print(result)

        print("Congratulations! Puzzle completed!")
        self.board.display_board(
            [(word, [(x,y) for y in range(self.board.height) for x in range(self.board.width) if self.board.board[y][x] in word], 'blue') for word in self.theme_words] +
            [(self.spangram, [(x,y) for y in range(self.board.height) for x in range(self.board.width) if self.board.board[y][x] in self.spangram], 'yellow')]
        )
