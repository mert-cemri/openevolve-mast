        elif direction == 'down':
            answer, (row, col) = self.answers_down.get(clue_number, ("", ()))
            if row + len(word) > len(self.grid):
                print("Word exceeds grid boundaries vertically.")
                return False
        else:
            print("Invalid direction specified.")
            return False
        return word == answer

