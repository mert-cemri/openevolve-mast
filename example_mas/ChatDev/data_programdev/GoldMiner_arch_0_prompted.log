        for obj in objects:
            if abs(obj.position - self.position) <= 1:
                return obj
        return None
    def reel_in(self, obj):
        print(f"Reeling in {obj.type}...")
        time.sleep(obj.weight)  # Simulate reel-in time based on weight
        return obj.value
