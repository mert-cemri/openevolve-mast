class Claw:
    def __init__(self):
        self.position = 0
        self.direction = 1  # 1 for right, -1 for left
        self.speed = 1  # Claw speed
    def move(self):
        self.position += self.direction * self.speed
        if self.position >= 10 or self.position <= 0:
            self.direction *= -1
        print(f"Claw position: {self.position}")
