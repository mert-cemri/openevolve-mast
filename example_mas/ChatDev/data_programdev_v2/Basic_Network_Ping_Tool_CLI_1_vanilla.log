import tkinter as tk
from tkinter import messagebox
import subprocess
import platform

class PingTool:
    def __init__(self, master):
        self.master = master
        master.title("Ping Tool")

        self.label = tk.Label(master, text="Enter IP address or hostname:")
        self.label.pack()

        self.entry = tk.Entry(master)
        self.entry.pack()

        self.label_count = tk.Label(master, text="Number of echo requests:")
        self.label_count.pack()

        self.entry_count = tk.Entry(master)
        self.entry_count.pack()

        self.ping_button = tk.Button(master, text="Ping", command=self.ping)
        self.ping_button.pack()

        self.results_text = tk.Text(master, height=10, width=50)
        self.results_text.pack()

    def ping(self):
        address = self.entry.get()
        count = self.entry_count.get()

        if not address:
            messagebox.showerror("Input Error", "Please enter a valid IP address or hostname.")
            return

        if not count.isdigit():
            messagebox.showerror("Input Error", "Please enter a valid number for echo requests.")
            return

        count = int(count)
        param = '-n' if platform.system().lower() == 'windows' else '-c'
        command = ['ping', param, str(count), address]

        try:
            output = subprocess.check_output(command, stderr=subprocess.STDOUT, universal_newlines=True)
            results = self.parse_ping_output(output)
            self.display_results(results)
        except subprocess.CalledProcessError as e:
            messagebox.showerror("Ping Error", f"Failed to ping {address}. Error: {e.output}")

    def parse_ping_output(self, output):
        lines = output.splitlines()
        times = []
        summary = ""
        for line in lines:
            if "time=" in line:
                time_part = line.split("time=")[-1]
                time_value = time_part.split()[0]
                times.append(time_value)
            if "min/avg/max" in line or "Minimum" in line:
                summary = line
        return times, summary

    def display_results(self, results):
        times, summary = results
        self.results_text.delete(1.0, tk.END)
        for time in times:
            self.results_text.insert(tk.END, f"Round-trip time: {time}\n")
        self.results_text.insert(tk.END, f"\nSummary:\n{summary}\n")

if __name__ == "__main__":
    root = tk.Tk()
    ping_tool = PingTool(root)
    root.mainloop()
```

