    def __init__(self, root):
        '''
        Initializes the GUI application.
        
        :param root: The root window of the tkinter application.
        '''
        self.root = root
        self.root.title("Process Killer")
        self.process_manager = ProcessManager()
        self.create_widgets()

    def create_widgets(self):
        '''
        Sets up the GUI components.
        '''
        self.label = tk.Label(self.root, text="Enter Process Name:")
        self.label.pack()

        self.entry = tk.Entry(self.root)
        self.entry.pack()

        self.search_button = tk.Button(self.root, text="Search", command=self.search_processes)
        self.search_button.pack()

        self.process_listbox = tk.Listbox(self.root, selectmode=tk.SINGLE)
        self.process_listbox.pack(fill=tk.BOTH, expand=True)

        self.kill_button = tk.Button(self.root, text="Kill Process", command=self.confirm_and_kill)
        self.kill_button.pack()

    def search_processes(self):
        '''
        Handles the search functionality to find processes by name.
        '''
        process_name = self.entry.get()
        self.process_listbox.delete(0, tk.END)
        processes = self.process_manager.find_processes_by_name(process_name)
        for proc in processes:
            self.process_listbox.insert(tk.END, f"{proc.info['name']} (PID: {proc.info['pid']})")

    def confirm_and_kill(self):
        '''
        Confirms and kills the selected process.
        '''
        selected = self.process_listbox.curselection()
        if not selected:
            messagebox.showwarning("Warning", "No process selected.")
            return

        process_info = self.process_listbox.get(selected[0])
        pid = int(process_info.split("PID: ")[1][:-1])

        if messagebox.askyesno("Confirmation", f"Are you sure you want to kill the process: {process_info}?"):
            success = self.process_manager.kill_process(pid)
            if success:
                messagebox.showinfo("Success", "Process killed successfully.")
                self.search_processes()
            else:
                messagebox.showerror("Error", "Failed to kill the process.")

if __name__ == "__main__":
    root = tk.Tk()
    app = ProcessKillerApp(root)
    root.mainloop()
```

