```python
'''

Manages the GUI and user interactions for the library application.

'''

import tkinter as tk
from tkinter import messagebox
from library import Library
from book import Book
from data_handler import DataHandler

class LibraryManager:
    def __init__(self):
        self.library = Library()
        self.data_handler = DataHandler('library_data.json')
        self.library.books = self.data_handler.load_data()
        self.root = tk.Tk()
        self.root.title("Book Library Manager")

    def run(self):
        self.setup_gui()
        self.root.mainloop()

    def setup_gui(self):
        # Add Book
        tk.Label(self.root, text="Title").grid(row=0, column=0)
        self.title_entry = tk.Entry(self.root)
        self.title_entry.grid(row=0, column=1)

        tk.Label(self.root, text="Author").grid(row=1, column=0)
        self.author_entry = tk.Entry(self.root)
        self.author_entry.grid(row=1, column=1)

        tk.Label(self.root, text="ISBN").grid(row=2, column=0)
        self.isbn_entry = tk.Entry(self.root)
        self.isbn_entry.grid(row=2, column=1)

        tk.Button(self.root, text="Add Book", command=self.add_book_gui).grid(row=3, column=0, columnspan=2)

        # List Books
        tk.Button(self.root, text="List Books", command=self.list_books_gui).grid(row=4, column=0, columnspan=2)

        # Search Books
        tk.Label(self.root, text="Search").grid(row=5, column=0)
        self.search_entry = tk.Entry(self.root)
        self.search_entry.grid(row=5, column=1)
        tk.Button(self.root, text="Search", command=self.search_books_gui).grid(row=6, column=0, columnspan=2)

        # Mark Book
        tk.Label(self.root, text="ISBN to Mark").grid(row=7, column=0)
        self.mark_isbn_entry = tk.Entry(self.root)
        self.mark_isbn_entry.grid(row=7, column=1)
        tk.Button(self.root, text="Mark Read/Unread", command=self.mark_book_gui).grid(row=8, column=0, columnspan=2)

    def add_book_gui(self):
        title = self.title_entry.get()
        author = self.author_entry.get()
        isbn = self.isbn_entry.get()
        if title and author and isbn:
            book = Book(title, author, isbn)
            self.library.add_book(book)
            self.data_handler.save_data(self.library.books)
            messagebox.showinfo("Success", "Book added successfully!")
        else:
            messagebox.showwarning("Input Error", "Please fill all fields.")

    def list_books_gui(self):
        books = self.library.list_books()
        book_list = "\n".join(str(book) for book in books)
        messagebox.showinfo("Book List", book_list if book_list else "No books in the library.")

    def search_books_gui(self):
        query = self.search_entry.get()
        if query:
            results = self.library.search_books(query)
            result_list = "\n".join(str(book) for book in results)
            messagebox.showinfo("Search Results", result_list if result_list else "No matching books found.")
        else:
            messagebox.showwarning("Input Error", "Please enter a search query.")

    def mark_book_gui(self):
        isbn = self.mark_isbn_entry.get()
        book = self.library.get_book(isbn)
        if book:
            if book.read:
                book.mark_as_unread()
                messagebox.showinfo("Success", f"Marked '{book.title}' as unread.")
            else:
                book.mark_as_read()
                messagebox.showinfo("Success", f"Marked '{book.title}' as read.")
            self.data_handler.save_data(self.library.books)
        else:
            messagebox.showwarning("Not Found", "Book not found.")
```

This implementation provides a simple GUI application for managing a book library, allowing users to add, list, search, and mark books as read or unread. The data is stored locally in a JSON file.

