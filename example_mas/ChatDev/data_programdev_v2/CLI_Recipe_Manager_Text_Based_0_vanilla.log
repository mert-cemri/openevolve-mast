import tkinter as tk
from tkinter import messagebox, simpledialog
from recipe_manager import RecipeManager

class RecipeApp:
    def __init__(self):
        self.manager = RecipeManager()
        self.root = tk.Tk()
        self.root.title("Recipe Manager")

    def run(self):
        self.create_main_menu()
        self.root.mainloop()

    def create_main_menu(self):
        frame = tk.Frame(self.root)
        frame.pack(pady=20)

        add_button = tk.Button(frame, text="Add Recipe", command=self.add_recipe_gui)
        add_button.grid(row=0, column=0, padx=10)

        search_button = tk.Button(frame, text="Search Recipes", command=self.search_recipes_gui)
        search_button.grid(row=0, column=1, padx=10)

    def add_recipe_gui(self):
        name = simpledialog.askstring("Input", "Enter recipe name:")
        if not name:
            return

        ingredients = simpledialog.askstring("Input", "Enter ingredients (comma separated):")
        if not ingredients:
            return

        instructions = simpledialog.askstring("Input", "Enter instructions:")
        if not instructions:
            return

        self.manager.add_recipe(name, ingredients.split(','), instructions)
        messagebox.showinfo("Success", "Recipe added successfully!")

    def search_recipes_gui(self):
        search_term = simpledialog.askstring("Search", "Enter recipe name or ingredient:")
        if not search_term:
            return

        results_by_name = self.manager.search_recipes_by_name(search_term)
        results_by_ingredient = self.manager.search_recipes_by_ingredient(search_term)

        results = {**results_by_name, **results_by_ingredient}

        if not results:
            messagebox.showinfo("No Results", "No recipes found.")
            return

        result_str = "\n".join(results.keys())
        selected_recipe = simpledialog.askstring("Results", f"Found recipes:\n{result_str}\n\nEnter recipe name to view details:")

        if selected_recipe in results:
            self.view_recipe_details_gui(selected_recipe)
        else:
            messagebox.showinfo("Error", "Recipe not found.")

    def view_recipe_details_gui(self, name):
        details = self.manager.get_recipe_details(name)
        if details:
            ingredients = ", ".join(details['ingredients'])
            instructions = details['instructions']
            messagebox.showinfo("Recipe Details", f"Name: {name}\nIngredients: {ingredients}\nInstructions: {instructions}")
        else:
            messagebox.showinfo("Error", "Recipe not found.")
