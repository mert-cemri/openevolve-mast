            with open("tasks.txt", "r") as file:
                for line in file:
                    date, time_str, command = line.strip().split(',')
                    self.add_task(date, time_str, command)
        except FileNotFoundError:
            pass
    def save_tasks(self):
        with open("tasks.txt", "w") as file:
            for task in self.tasks:
