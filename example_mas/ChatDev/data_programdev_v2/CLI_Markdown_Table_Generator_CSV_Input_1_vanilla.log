                markdown_table = convert_csv_to_markdown(csv_content)
                self.display_markdown(markdown_table)
            except Exception as e:
                messagebox.showerror("Error", f"Failed to read file: {e}")

    def display_markdown(self, markdown_table):
        '''
        Display the converted Markdown table in the text area.
        '''
        self.text_area.delete(1.0, tk.END)
        self.text_area.insert(tk.END, markdown_table)

if __name__ == "__main__":
    root = tk.Tk()
    app = MarkdownTableApp(root)
    root.mainloop()
```

