        text = re.sub(r'\*(.+?)\*', r'<em>\1</em>', text)
        return text
    def convert_lists(self, text):
        # Convert list items
        text = re.sub(r'^\*\s+(.+)', r'<li>\1</li>', text, flags=re.MULTILINE)
