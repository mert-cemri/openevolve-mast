        tk.Button(self.root, text="Remove Song", command=self.remove_song).grid(row=4, column=1)
        
        # Playlist display
        self.playlist_listbox = tk.Listbox(self.root, width=50)
        self.playlist_listbox.grid(row=5, column=0, columnspan=3)
        
        tk.Button(self.root, text="View Playlist", command=self.view_playlist).grid(row=6, column=0)
        
        self.update_playlist_display()
    
    def create_playlist(self):
        name = self.playlist_name_var.get()
        if name:
            self.manager.create_playlist(name)
            self.update_playlist_display()
    
