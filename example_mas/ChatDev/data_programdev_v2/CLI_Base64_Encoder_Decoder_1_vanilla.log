        self.decode_button.pack()

        self.result_label = tk.Label(self.root, text="Result:")
        self.result_label.pack()

        self.result_text = tk.Text(self.root, height=5, width=50)
        self.result_text.pack()

    def on_encode(self):
        '''
        Handles the encode button click event.
        '''
        input_string = self.input_entry.get()
        result = Base64Tool.encode_string(input_string)
        self.result_text.delete(1.0, tk.END)
        self.result_text.insert(tk.END, result)

    def on_decode(self):
        '''
        Handles the decode button click event.
        '''
        input_string = self.input_entry.get()
        result = Base64Tool.decode_string(input_string)
        self.result_text.delete(1.0, tk.END)
        self.result_text.insert(tk.END, result)

if __name__ == "__main__":
    root = tk.Tk()
    app = Base64App(root)
    root.mainloop()
```

