import tkinter as tk
from tkinter import filedialog, messagebox
import os

class SpellCheckerApp:
    def __init__(self, root):
        self.root = root
        self.root.title("Spell Checker")
        
        self.text_area = tk.Text(self.root, wrap='word', height=20, width=60)
        self.text_area.pack(padx=10, pady=10)
        
        self.check_button = tk.Button(self.root, text="Check Spelling", command=self.check_spelling)
        self.check_button.pack(pady=5)
        
        self.open_button = tk.Button(self.root, text="Open File", command=self.open_file)
        self.open_button.pack(pady=5)
        
        self.dictionary = set()
        self.load_dictionary()
        
    def load_dictionary(self):
        '''Loads words from a local dictionary file into a set for quick lookup.'''
        try:
            with open('dictionary.txt', 'r') as file:
                for line in file:
                    self.dictionary.add(line.strip().lower())
        except FileNotFoundError:
            messagebox.showerror("Error", "Dictionary file not found.")
    
    def check_spelling(self):
        '''Checks the input text for misspellings and highlights them.'''
        text_content = self.text_area.get("1.0", tk.END).lower()
        words = text_content.split()
        self.highlight_misspellings(words)
    
    def open_file(self):
        '''Opens a file dialog to select a text file for spell checking.'''
        file_path = filedialog.askopenfilename(filetypes=[("Text Files", "*.txt")])
        if file_path:
            with open(file_path, 'r') as file:
                content = file.read()
                self.text_area.delete("1.0", tk.END)
                self.text_area.insert(tk.END, content)
    
    def highlight_misspellings(self, words):
        '''Highlights misspelled words in the text widget.'''
        self.text_area.tag_remove("misspelled", "1.0", tk.END)
        
        for word in words:
            if word not in self.dictionary:
                start_idx = "1.0"
                while True:
                    start_idx = self.text_area.search(word, start_idx, nocase=1, stopindex=tk.END)
                    if not start_idx:
                        break
                    end_idx = f"{start_idx}+{len(word)}c"
                    self.text_area.tag_add("misspelled", start_idx, end_idx)
                    start_idx = end_idx
        self.text_area.tag_config("misspelled", foreground="red")

if __name__ == "__main__":
    root = tk.Tk()
    app = SpellCheckerApp(root)
    root.mainloop()
