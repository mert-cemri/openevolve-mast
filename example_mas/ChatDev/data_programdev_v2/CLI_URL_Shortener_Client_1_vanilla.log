import tkinter as tk
import requests

class URLShortenerApp:
    def __init__(self, root):
        '''
        Initialize the URL Shortener application with a GUI.
        
        Parameters:
        root (tk.Tk): The root window of the tkinter application.
        '''
        self.root = root
        self.root.title("URL Shortener")

        # Create and place widgets
        self.url_label = tk.Label(root, text="Enter URL:")
        self.url_label.pack()

        self.url_entry = tk.Entry(root, width=50)
        self.url_entry.pack()

        self.shorten_button = tk.Button(root, text="Shorten", command=self.on_shorten_button_click)
        self.shorten_button.pack()

        self.result_label = tk.Label(root, text="")
        self.result_label.pack()

    def on_shorten_button_click(self):
        '''
        Handle the shorten button click event.
        '''
        long_url = self.url_entry.get()
        if long_url:
            short_url = self.shorten_url(long_url)
            self.result_label.config(text=f"Shortened URL: {short_url}")
        else:
            self.result_label.config(text="Please enter a valid URL.")

    def shorten_url(self, long_url):
        '''
        Shorten the provided URL using the TinyURL API.
        
        Parameters:
        long_url (str): The long URL to be shortened.
        
        Returns:
        str: The shortened URL.
        '''
        try:
            response = requests.get(f"http://tinyurl.com/api-create.php?url={long_url}")
            if response.status_code == 200:
                return response.text
            else:
                return "Error: Unable to shorten URL."
        except requests.exceptions.RequestException as e:
            return f"Error: {e}"

if __name__ == "__main__":
    root = tk.Tk()
    app = URLShortenerApp(root)
    root.mainloop()
