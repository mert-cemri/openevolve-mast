import tkinter as tk
from tkinter import ttk

def convert_length(value, from_unit, to_unit):
    '''
    Convert length from one unit to another.
    
    Parameters:
    value (float): The numerical value to convert.
    from_unit (str): The unit of the input value.
    to_unit (str): The unit to convert the value to.
    
    Returns:
    float: The converted value.
    '''
    # Conversion factors relative to meters
    conversion_factors = {
        'm': 1.0,
        'cm': 100.0,
        'in': 39.3701,
        'ft': 3.28084
    }
    
    # Convert the input value to meters
    value_in_meters = value / conversion_factors[from_unit]
    
    # Convert from meters to the target unit
    converted_value = value_in_meters * conversion_factors[to_unit]
    
    return converted_value

class UnitConverterApp:
    '''
    A GUI application for converting lengths between different units.
    '''
    
    def __init__(self, root):
        self.root = root
        self.root.title("Length Unit Converter")
        self.create_widgets()
    
    def create_widgets(self):
        '''
        Create and place the widgets in the application window.
        '''
        # Entry for the value to convert
        self.value_entry = tk.Entry(self.root)
        self.value_entry.grid(row=0, column=1, padx=10, pady=10)
        
        # Dropdown for the source unit
        self.from_unit_var = tk.StringVar(value='m')
        self.from_unit_dropdown = ttk.Combobox(self.root, textvariable=self.from_unit_var)
        self.from_unit_dropdown['values'] = ('m', 'cm', 'in', 'ft')
        self.from_unit_dropdown.grid(row=0, column=2, padx=10, pady=10)
        
        # Dropdown for the target unit
        self.to_unit_var = tk.StringVar(value='cm')
        self.to_unit_dropdown = ttk.Combobox(self.root, textvariable=self.to_unit_var)
        self.to_unit_dropdown['values'] = ('m', 'cm', 'in', 'ft')
        self.to_unit_dropdown.grid(row=0, column=3, padx=10, pady=10)
        
        # Button to perform the conversion
        self.convert_button = tk.Button(self.root, text="Convert", command=self.convert_units)
        self.convert_button.grid(row=0, column=4, padx=10, pady=10)
        
        # Label to display the result
        self.result_label = tk.Label(self.root, text="Result: ")
        self.result_label.grid(row=1, column=1, columnspan=4, padx=10, pady=10)
    
    def convert_units(self):
        '''
        Convert the input value from the source unit to the target unit and display the result.
        '''
        try:
            value = float(self.value_entry.get())
            from_unit = self.from_unit_var.get()
            to_unit = self.to_unit_var.get()
            result = convert_length(value, from_unit, to_unit)
            self.result_label.config(text=f"Result: {result:.4f} {to_unit}")
        except ValueError:
            self.result_label.config(text="Invalid input. Please enter a numeric value.")

if __name__ == "__main__":
    root = tk.Tk()
    app = UnitConverterApp(root)
    root.mainloop()
```

