        numbered_content = "\n".join(f"{i+1}: {line}" for i, line in enumerate(content))
        
        self.text_area.delete(1.0, tk.END)
        self.text_area.insert(tk.END, numbered_content)
    
    def save_output(self):
        if not self.file_path:
