'''

Main application file for the translation CLI client using tkinter for GUI.

'''

import tkinter as tk
from tkinter import messagebox
from translator import translate

class TranslateApp:
    def __init__(self, root):
        self.root = root
        self.root.title("Language Translator")

        # Input text
        self.input_label = tk.Label(root, text="Enter text:")
        self.input_label.pack()
        self.input_text = tk.Text(root, height=10, width=50)
        self.input_text.pack()

        # Target language
        self.language_label = tk.Label(root, text="Target Language (e.g., 'es' for Spanish):")
        self.language_label.pack()
        self.language_entry = tk.Entry(root)
        self.language_entry.pack()

        # Translate button
        self.translate_button = tk.Button(root, text="Translate", command=self.translate_text)
        self.translate_button.pack()

        # Output text
        self.output_label = tk.Label(root, text="Translated text:")
        self.output_label.pack()
        self.output_text = tk.Text(root, height=10, width=50, state='disabled')
        self.output_text.pack()

    def translate_text(self):
        text = self.input_text.get("1.0", tk.END).strip()
        target_language = self.language_entry.get().strip()

        if not text or not target_language:
            messagebox.showerror("Input Error", "Please provide both text and target language.")
            return

        try:
            translated_text = translate(text, target_language)
            self.output_text.config(state='normal')
            self.output_text.delete("1.0", tk.END)
            self.output_text.insert(tk.END, translated_text)
            self.output_text.config(state='disabled')
        except Exception as e:
            messagebox.showerror("Translation Error", str(e))

if __name__ == "__main__":
    root = tk.Tk()
    app = TranslateApp(root)
    root.mainloop()
```

