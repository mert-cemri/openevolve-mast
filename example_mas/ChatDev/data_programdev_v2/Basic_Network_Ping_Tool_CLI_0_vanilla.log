import tkinter as tk
from tkinter import messagebox
from utils import run_ping_command

class PingTool:
    def __init__(self):
        self.root = tk.Tk()
        self.root.title("Ping Tool")

        # Input fields
        self.address_label = tk.Label(self.root, text="IP Address/Hostname:")
        self.address_label.pack()
        self.address_entry = tk.Entry(self.root)
        self.address_entry.pack()

        self.count_label = tk.Label(self.root, text="Number of Pings:")
        self.count_label.pack()
        self.count_entry = tk.Entry(self.root)
        self.count_entry.pack()

        # Ping button
        self.ping_button = tk.Button(self.root, text="Ping", command=self.ping)
        self.ping_button.pack()

        # Results display
        self.results_text = tk.Text(self.root, height=10, width=50)
        self.results_text.pack()

        self.root.mainloop()

