'''

Main application file for the CLI translation tool with a GUI using tkinter.

'''

import tkinter as tk
from tkinter import ttk, messagebox
from translator import translate

class TranslateApp:
    def __init__(self, root):
        '''
        Initialize the main application with GUI components.
        '''
        self.root = root
        self.root.title("Language Translator")

        # Input text
        self.input_label = ttk.Label(root, text="Enter text:")
        self.input_label.pack(pady=5)
        self.input_text = tk.Text(root, height=10, width=50)
        self.input_text.pack(pady=5)

        # Target language
        self.language_label = ttk.Label(root, text="Target Language (e.g., 'es' for Spanish):")
        self.language_label.pack(pady=5)
        self.language_entry = ttk.Entry(root)
        self.language_entry.pack(pady=5)

        # Translate button
        self.translate_button = ttk.Button(root, text="Translate", command=self.translate_text)
        self.translate_button.pack(pady=20)

        # Output text
        self.output_label = ttk.Label(root, text="Translated text:")
        self.output_label.pack(pady=5)
        self.output_text = tk.Text(root, height=10, width=50, state='disabled')
        self.output_text.pack(pady=5)

    def translate_text(self):
        '''
        Translate the input text to the target language and display the result.
        '''
        input_text = self.input_text.get("1.0", tk.END).strip()
        target_language = self.language_entry.get().strip()

        if not input_text or not target_language:
            messagebox.showerror("Error", "Please enter both text and target language.")
            return

        try:
            translated_text = translate(input_text, target_language)
            self.output_text.config(state='normal')
            self.output_text.delete("1.0", tk.END)
            self.output_text.insert(tk.END, translated_text)
            self.output_text.config(state='disabled')
        except Exception as e:
            messagebox.showerror("Error", str(e))

if __name__ == "__main__":
    root = tk.Tk()
    app = TranslateApp(root)
    root.mainloop()
```

