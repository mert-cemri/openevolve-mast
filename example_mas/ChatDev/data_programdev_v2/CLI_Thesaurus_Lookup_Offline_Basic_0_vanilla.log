'''

Main application file for the CLI thesaurus lookup tool using tkinter for GUI.

'''

import tkinter as tk
from thesaurus import Thesaurus

class ThesaurusApp:
    def __init__(self, master):
        self.master = master
        self.master.title("Thesaurus Lookup Tool")
        
        self.label = tk.Label(master, text="Enter a word:")
        self.label.pack()
        
        self.entry = tk.Entry(master)
        self.entry.pack()
        
        self.lookup_button = tk.Button(master, text="Lookup", command=self.lookup_word)
        self.lookup_button.pack()
        
        self.result_text = tk.Text(master, height=10, width=50)
        self.result_text.pack()
        
        self.thesaurus = Thesaurus('thesaurus.txt')
    
    def lookup_word(self):
        word = self.entry.get()
        synonyms = self.thesaurus.get_synonyms(word)
        antonyms = self.thesaurus.get_antonyms(word)
        
        self.result_text.delete(1.0, tk.END)
        if synonyms:
            self.result_text.insert(tk.END, f"Synonyms for '{word}': {', '.join(synonyms)}\n")
        else:
            self.result_text.insert(tk.END, f"No synonyms found for '{word}'.\n")
        
        if antonyms:
            self.result_text.insert(tk.END, f"Antonyms for '{word}': {', '.join(antonyms)}\n")
        else:
            self.result_text.insert(tk.END, f"No antonyms found for '{word}'.\n")

if __name__ == "__main__":
    root = tk.Tk()
    app = ThesaurusApp(root)
    root.mainloop()
```

