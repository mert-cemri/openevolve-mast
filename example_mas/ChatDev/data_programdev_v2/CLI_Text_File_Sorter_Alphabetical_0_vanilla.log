'''

Main application file for the CLI program with GUI to sort lines of a text file.

'''

import tkinter as tk
from tkinter import filedialog, messagebox
from sort_functions import sort_alphabetically, sort_reverse_alphabetically
from file_functions import read_file, write_file

class SortApp:
    def __init__(self, root):
        self.root = root
        self.root.title("Text File Sorter")

        self.file_path = tk.StringVar()
        self.sort_order = tk.StringVar(value="Alphabetical")

        self.create_widgets()

    def create_widgets(self):
        tk.Label(self.root, text="Select File:").grid(row=0, column=0, padx=10, pady=10)
        tk.Entry(self.root, textvariable=self.file_path, width=50).grid(row=0, column=1, padx=10, pady=10)
        tk.Button(self.root, text="Browse", command=self.open_file).grid(row=0, column=2, padx=10, pady=10)

        tk.Label(self.root, text="Sort Order:").grid(row=1, column=0, padx=10, pady=10)
        tk.Radiobutton(self.root, text="Alphabetical", variable=self.sort_order, value="Alphabetical").grid(row=1, column=1, padx=10, pady=10, sticky='w')
        tk.Radiobutton(self.root, text="Reverse Alphabetical", variable=self.sort_order, value="Reverse").grid(row=1, column=1, padx=10, pady=10)

        tk.Button(self.root, text="Sort and Save", command=self.sort_lines).grid(row=2, column=1, padx=10, pady=10)

    def open_file(self):
        file_path = filedialog.askopenfilename(filetypes=[("Text files", "*.txt")])
        if file_path:
            self.file_path.set(file_path)

    def sort_lines(self):
        file_path = self.file_path.get()
        if not file_path:
            messagebox.showerror("Error", "Please select a file.")
            return

        try:
            lines = read_file(file_path)
            if self.sort_order.get() == "Alphabetical":
                sorted_lines = sort_alphabetically(lines)
            else:
                sorted_lines = sort_reverse_alphabetically(lines)

            save_path = filedialog.asksaveasfilename(defaultextension=".txt", filetypes=[("Text files", "*.txt")])
            if save_path:
                write_file(save_path, sorted_lines)
                messagebox.showinfo("Success", "File sorted and saved successfully.")
        except Exception as e:
            messagebox.showerror("Error", str(e))

if __name__ == "__main__":
    root = tk.Tk()
    app = SortApp(root)
    root.mainloop()
```

