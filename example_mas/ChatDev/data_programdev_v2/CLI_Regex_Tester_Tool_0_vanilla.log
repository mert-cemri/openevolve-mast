import tkinter as tk
from tkinter import ttk, messagebox
import re

class RegexTesterApp:
    '''
    A class to create a GUI application for testing regular expressions.
    '''
    
    def __init__(self, root):
        '''
        Initialize the application with the main window and widgets.
        '''
        self.root = root
        self.root.title("CLI Regex Tester")
        
        self.create_widgets()
    
    def create_widgets(self):
        '''
        Create and arrange the widgets in the GUI.
        '''
        # Regular expression input
        self.regex_label = ttk.Label(self.root, text="Regular Expression:")
        self.regex_label.grid(row=0, column=0, padx=5, pady=5, sticky="w")
        
        self.regex_entry = ttk.Entry(self.root, width=50)
        self.regex_entry.grid(row=0, column=1, padx=5, pady=5, sticky="ew")
        
        # Test string input
        self.string_label = ttk.Label(self.root, text="Test String:")
        self.string_label.grid(row=1, column=0, padx=5, pady=5, sticky="w")
        
        self.string_entry = ttk.Entry(self.root, width=50)
        self.string_entry.grid(row=1, column=1, padx=5, pady=5, sticky="ew")
        
        # Test button
        self.test_button = ttk.Button(self.root, text="Test", command=self.test_regex)
        self.test_button.grid(row=2, column=0, columnspan=2, pady=10)
        
        # Result display
        self.result_label = ttk.Label(self.root, text="Result:")
        self.result_label.grid(row=3, column=0, padx=5, pady=5, sticky="nw")
        
        self.result_text = tk.Text(self.root, height=10, width=60, state='disabled')
        self.result_text.grid(row=3, column=1, padx=5, pady=5, sticky="ew")
    
    def test_regex(self):
        '''
        Test the regular expression against the input string and display the results.
        '''
        regex = self.regex_entry.get()
        test_string = self.string_entry.get()
        
        try:
            pattern = re.compile(regex)
            match = pattern.search(test_string)
            
            self.result_text.config(state='normal')
            self.result_text.delete(1.0, tk.END)
            
            if match:
                self.result_text.insert(tk.END, "Match found!\n")
                self.result_text.insert(tk.END, f"Matched groups: {match.groups()}\n")
            else:
                self.result_text.insert(tk.END, "No match found.\n")
            
            self.result_text.config(state='disabled')
        
        except re.error as e:
            messagebox.showerror("Regex Error", f"Invalid regular expression: {e}")

def main():
    '''
    The entry point of the application.
    '''
    root = tk.Tk()
    app = RegexTesterApp(root)
    root.mainloop()

if __name__ == "__main__":
    main()
