'''

Handles task scheduling and execution.

'''

import schedule
import time
import threading

class TaskScheduler:
    def __init__(self):
        self.tasks = []

    def add_task(self, command, date, time):
        task_time = f"{date} {time}"
        schedule.every().day.at(task_time).do(self.execute_command, command)
        self.tasks.append((command, task_time))
        self.save_tasks()

    def execute_command(self, command):
        print(f"Executing: {command}")
        # Here you would use os.system or subprocess to execute the command
        # os.system(command)

    def load_tasks(self):
        try:
            with open("tasks.txt", "r") as file:
                for line in file:
                    command, task_time = line.strip().split(',')
                    self.tasks.append((command, task_time))
                    schedule.every().day.at(task_time).do(self.execute_command, command)
        except FileNotFoundError:
            pass

    def save_tasks(self):
        with open("tasks.txt", "w") as file:
            for command, task_time in self.tasks:
                file.write(f"{command},{task_time}\n")

    def run_pending(self):
        while True:
            schedule.run_pending()
            time.sleep(1)

    def start_scheduler(self):
        thread = threading.Thread(target=self.run_pending)
        thread.start()
