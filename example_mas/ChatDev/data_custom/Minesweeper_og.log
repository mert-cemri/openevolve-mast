        for row in range(self.size):
            for col in range(self.size):
                if self.board[row][col] == 'M':
                    continue
                count = 0
                for r in range(max(0, row-1), min(self.size, row+2)):
                    for c in range(max(0, col-1), min(self.size, col+2)):
                        if self.board[r][c] == 'M':
