class Grid:
    def __init__(self, size):
        self.size = size
        self.grid = [[0] * size for _ in range(size)]

    def get_empty_positions(self):
        return [(x, y) for x in range(self.size) for y in range(self.size) if self.grid[x][y] == 0]

    def set_tile(self, x, y, value):
        self.grid[x][y] = value

    def move(self, direction):
        moved = False
        if direction == 'up':
            moved = self._move_vertical(reverse=False)
        elif direction == 'down':
            moved = self._move_vertical(reverse=True)
        elif direction == 'left':
            moved = self._move_horizontal(reverse=False)
        elif direction == 'right':
            moved = self._move_horizontal(reverse=True)
        return moved

    def _move_vertical(self, reverse):
        moved = False
        for col in range(self.size):
            tiles = [self.grid[row][col] for row in range(self.size) if self.grid[row][col] != 0]
            if reverse:
                tiles.reverse()
            merged = self._merge_tiles(tiles)
            if reverse:
                merged.reverse()
            for row in range(self.size):
                value = merged[row] if row < len(merged) else 0
                if self.grid[row][col] != value:
                    moved = True
                self.grid[row][col] = value
        return moved

    def _move_horizontal(self, reverse):
        moved = False
        for row in range(self.size):
            tiles = [self.grid[row][col] for col in range(self.size) if self.grid[row][col] != 0]
            if reverse:
                tiles.reverse()
            merged = self._merge_tiles(tiles)
            if reverse:
                merged.reverse()
            for col in range(self.size):
                value = merged[col] if col < len(merged) else 0
                if self.grid[row][col] != value:
                    moved = True
                self.grid[row][col] = value
        return moved

    def _merge_tiles(self, tiles):
        if not tiles:
            return []
        merged = []
        skip = False
        for i in range(len(tiles)):
            if skip:
                skip = False
                continue
            if i + 1 < len(tiles) and tiles[i] == tiles[i + 1]:
                merged.append(tiles[i] * 2)
                skip = True
            else:
                merged.append(tiles[i])
        return merged

    def can_move(self):
        if any(0 in row for row in self.grid):
            return True
        for x in range(self.size):
            for y in range(self.size - 1):
                if self.grid[x][y] == self.grid[x][y + 1]:
                    return True
                if self.grid[y][x] == self.grid[y + 1][x]:
                    return True
        return False

    def render(self):
        for row in self.grid:
            print(' '.join(str(num).rjust(4) for num in row))
        print()
