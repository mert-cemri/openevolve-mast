                self.swap_candies(self.selected_candy, (row, col))
                if not self.check_matches():
                    # Revert swap if no matches are found
                    self.swap_candies(self.selected_candy, (row, col))
                self.selected_candy = None
            else:
                self.selected_candy = (row, col)
        else:
            self.selected_candy = (row, col)
