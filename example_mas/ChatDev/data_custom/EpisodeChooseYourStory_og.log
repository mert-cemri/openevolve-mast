class Game:
    def __init__(self):
        self.player = Player()
        self.story_nodes = self.load_story_nodes()
        self.current_node = self.story_nodes[0]
        self.gui = None

    def load_story_nodes(self):
        with open('story_nodes.json', 'r') as file:
