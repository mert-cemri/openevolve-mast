        for row in self.grid:
            if '' in row:
                return False
        return True
    def check_winner(self):
        # Check horizontal, vertical, and diagonal lines for a winner
        for y in range(self.size):
            for x in range(self.size):
                if self.grid[y][x] and self.check_line(x, y):
